@component-name = "portal-content-management"
definition {

	property ci.retries.disabled = "true";
	property portal.release = "true";
	property portal.upstream = "true";
	property test.liferay.virtual.instance = "false";
	property test.run.type = "single";
	property testray.main.component.name = "Asset Publisher Widget";

	setUp {
		SignIn.signIn();

		var portalURL = PropsUtil.get("portal.url");

		AssertLocation(value1 = "${portalURL}/web/guest?SM_USER=test@liferay.com");

		SearchAdministration.executeReindex();
	}

	@description = "This is a test for LPS-125952. View displayed assets on Most Viewed Assets widget after upgrade from 706."
	@priority = 4
	test ViewDisplayedAssetsOnMostViewedAssetsAfterUpgrade706 {
		property data.archive.type = "data-archive-most-viewed-assets";
		property database.types = "mariadb,mysql,postgresql";
		property portal.version = "7.0.6";

		ValidateMostViewedAssetsUpgrade.viewOrderOfAssetsOnMostedViewedAssets();
	}

	@description = "This is a test for LPS-125952. View displayed assets on Most Viewed Assets widget after upgred from 701016."
	@priority = 4
	test ViewDisplayedAssetsOnMostViewedAssetsAfterUpgrade701016 {
		property data.archive.type = "data-archive-most-viewed-assets";
		property database.types = "db2,mariadb,mysql,oracle,postgresql,sqlserver";
		property portal.version = "7.0.10.16";

		ValidateMostViewedAssetsUpgrade.viewOrderOfAssetsOnMostedViewedAssets();
	}

}