			JSONRole.addRegularRole(
				roleKey = ${randomRoleKey},
				roleTitle = ${randomRoleKey});
		}

		task ("Define the permissions of regular role") {
			Permissions.definePermissionsViaJSONAPI(
				resourceNameActionList = "com.liferay.journal,ADD_FOLDER,ADD_FEED,ADD_ARTICLE,VIEW,UPDATE,SUBSCRIBE,PERMISSIONS,ADD_TEMPLATE,ADD_STRUCTURE:com.liferay.journal.model.JournalArticle,VIEW,UPDATE_DISCUSSION,UPDATE,SUBSCRIBE,PERMISSIONS,EXPIRE,DELETE_DISCUSSION,DELETE,ADD_DISCUSSION:com.liferay.portal.kernel.model.Group,VIEW_SITE_ADMINISTRATION:com.liferay.dynamic.data.mapping.model.DDMStructure-com.liferay.journal.model.JournalArticle,VIEW:com_liferay_journal_web_portlet_JournalPortlet,VIEW,PREFERENCES,PERMISSIONS,CONFIGURATION,ACCESS_IN_CONTROL_PANEL",
				roleTitle = ${randomRoleKey},
				roleType = "regular");
		}

		task ("Assign the regular roles to two new users") {
			for (var userScreenName : list "${userScreenName1},${userScreenName2}") {
				JSONUser.addUser(
					userEmailAddress = StringUtil.toLowerCase("${userScreenName}@liferay.com"),
