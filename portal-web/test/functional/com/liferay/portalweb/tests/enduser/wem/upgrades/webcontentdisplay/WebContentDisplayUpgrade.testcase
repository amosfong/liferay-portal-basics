@component-name = "portal-upgrades"
definition {

	property ci.retries.disabled = "true";
	property portal.release = "true";
	property portal.upstream = "true";
	property test.liferay.virtual.instance = "false";
	property test.run.type = "single";
	property testray.component.names = "Web Content Display,Upgrades Content Management";
	property testray.main.component.name = "Upgrades Content Management";

	setUp {
		SignIn.signIn();

		var portalURL = PropsUtil.get("portal.url");

		AssertLocation(value1 = "${portalURL}/web/guest?SM_USER=test@liferay.com");

		SearchAdministration.executeReindex();
	}

	@description = "This is a use case for LPS-71487. View Multi WCD After Upgrade 625"
	@priority = 5
	test ViewMultiWCDAfterUpgrade625 {
		property data.archive.type = "data-archive-multi-wcd-portlet";
		property database.types = "mysql";
		property portal.version = "6.2.5";

		Navigator.gotoPage(pageName = "Web Content Display Page");

		Portlet.viewDuplicatePG(
			portletName = "Web Content Display",
			totalPortletCount = 3);
	}

	@description = "This is a use case for LPS-71487. View Multi WCD Archive 621015"
	@priority = 5
	test ViewMultiWCDArchive621015 {
		property data.archive.type = "data-archive-multi-wcd-portlet";
		property database.types = "db2,mysql,oracle";
		property portal.version = "6.2.10.15";

		Navigator.gotoPage(pageName = "Web Content Display Page");

		Portlet.viewDuplicatePG(
			portletName = "Web Content Display",
			totalPortletCount = 3);
	}

	@description = "This is a use case for LPS-71487. View Multi WCD Archive 621021"
	@priority = 5
	test ViewMultiWCDArchive621021 {
		property data.archive.type = "data-archive-multi-wcd-portlet";
		property database.types = "db2,mysql,oracle,postgresql";
		property portal.version = "6.2.10.21";

		Navigator.gotoPage(pageName = "Web Content Display Page");

		Portlet.viewDuplicatePG(
			portletName = "Web Content Display",
			totalPortletCount = 3);
	}

}