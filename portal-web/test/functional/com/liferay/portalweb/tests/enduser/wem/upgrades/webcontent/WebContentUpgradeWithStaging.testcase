@component-name = "portal-upgrades"
definition {

	property ci.retries.disabled = "true";
	property database.types = "db2,mariadb,mysql,oracle,postgresql,sqlserver";
	property portal.release = "true";
	property portal.upstream = "true";
	property test.liferay.virtual.instance = "false";
	property test.run.type = "single";
	property testray.component.names = "Upgrades Staging,Upgrades Content Management,Web Content Administration";
	property testray.main.component.name = "Upgrades Content Management";

	setUp {
		SignIn.signIn();

		SearchAdministration.executeReindex();
	}

	@priority = 4
	test PublishStagingArchiveAfterUpgrade625 {
		property data.archive.type = "data-archive-staging-unpublished";
		property database.types = "mariadb,mysql,postgresql";
