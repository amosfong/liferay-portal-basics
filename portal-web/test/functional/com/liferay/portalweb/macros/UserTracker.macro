definition {

	@summary = "Navigate to the User Tracker Admin page with url navigation"
	macro openMonitoringAdmin(baseURL = null) {
		Navigator.openWithAppendToBaseURL(
			baseURL = ${baseURL},
			urlAppend = "group/guest/~/control_panel/manage?p_p_id=com_liferay_monitoring_web_portlet_MonitoringPortlet");
	}

	@summary = "Assert the accessed URL"
	macro viewAccessedURL(friendlyURL = null) {
		var key_friendlyURL = ${friendlyURL};

		Click(locator1 = "LexiconTable#TABLE_HITS_COUNT");

		Click(
			locator1 = "ListGroupItem#SESSION_ACCESSED_URLS",
			value1 = "ACCESSED URLS");

		ScrollWebElementIntoView(locator1 = "ListGroupItem#ACCESSED_FRIENDLY_URLS");

		AssertTextEquals.assertPartialText(
			locator1 = "ListGroupItem#ACCESSED_FRIENDLY_URLS",
			value1 = ${friendlyURL});
	}

}