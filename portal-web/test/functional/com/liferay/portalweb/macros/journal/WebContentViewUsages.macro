definition {

	@summary = "Default summary"
	macro clickEllipsisItem(item = null, page = null) {
		LexiconTable.clickEllipsisItem(
			item = ${item},
			tableEntry = ${page});
	}

	@summary = "Default summary"
	macro previewDraftInPage(page = null) {
		WebContentViewUsages.clickEllipsisItem(
			item = "Preview Draft in Page",
			page = ${page});
	}

	@summary = "Default summary"
	macro previewPendingInPage(page = null) {
		WebContentViewUsages.clickEllipsisItem(
			item = "Preview Pending in Page",
			page = ${page});
	}

	@summary = "Default summary"
	macro previewScheduledInPage(page = null) {
		WebContentViewUsages.clickEllipsisItem(
			item = "Preview Scheduled in Page",
			page = ${page});
	}

	@summary = "Default summary"
	macro viewInPage(page = null) {
		WebContentViewUsages.clickEllipsisItem(
			item = "View in Page",
			page = ${page});
	}

}