definition {

	@summary = "Default summary"
	macro _openDescriptiveMenulistCP(webContentTitle = null, webContentTitleEscapeCharacter = null, liveSite = null) {
		if (isSet(webContentTitleEscapeCharacter)) {
			var key_webContentTitle = ${webContentTitleEscapeCharacter};
		}
		else {
			var key_webContentTitle = ${webContentTitle};
		}

		LexiconEntry.changeDisplayStyle(displayStyle = "list");

		if (${liveSite} == "true") {
			AssertTextEquals.assertPartialText(
				locator1 = "WC#ENTRY_LIST_TITLE_ON_LIVE",
				value1 = ${webContentTitle});
		}
		else {
			AssertTextEquals.assertPartialText(
				locator1 = "WC#ENTRY_LIST_TITLE",
				value1 = ${webContentTitle});
		}

		Click.waitForMenuToggleJSClick(locator1 = "WC#ENTRY_LIST_MENULIST_ICON");
	}

	@summary = "Default summary"
	macro gotoAddCP() {
		LexiconEntry.gotoAdd();

		MenuItem.click(menuItem = "Basic Web Content");

		AssertVisible(locator1 = "TextInput#TITLE");

		WaitForVisible(locator1 = "WCEditWebContent#SIDEBAR");
	}

	@summary = "Default summary"
	macro gotoAddWithStructureCP(structureName = null) {
		LexiconEntry.gotoAdd();

		if (IsElementPresent(key_menuItem = ${structureName}, locator1 = "MenuItem#ANY_MENU_ITEM")) {
			MenuItem.click(menuItem = ${structureName});
		}
		else {
			Button.clickMore();

			IFrame.selectConfigurationFrame();

			LexiconEntry.gotoEntry(rowEntry = ${structureName});

			IFrame.selectTopFrame();
		}
	}

	@summary = "Default summary"
	macro gotoEditCP(webContentTitle = null, webContentTitleEscapeCharacter = null) {
		WebContentNavigator._openDescriptiveMenulistCP(
			webContentTitle = ${webContentTitle},
			webContentTitleEscapeCharacter = ${webContentTitleEscapeCharacter});

		MenuItem.click(menuItem = "Edit");

		AssertElementPresent(locator1 = "Icon#BACK");

		WaitForVisible(locator1 = "WCEditWebContent#SIDEBAR");

		WaitForVisible(locator1 = "Sidebar#FORM_BUILDER");
	}

	@summary = "Default summary"
	macro gotoEditFeed(feedName = null) {
		AssertClick(
			key_feedName = ${feedName},
			locator1 = "DDMSelectFeed#FEED_TABLE_NAME_LINK",
			value1 = ${feedName});

		SelectFrame(value1 = "relative=top");
	}

	@summary = "Default summary"
	macro gotoEditStructure(structureName = null) {
		WaitForElementPresent(
			key_ddlDataDefinitionName = ${structureName},
			locator1 = "DDMSelectStructure#DDM_STRUCTURE_TABLE_NAME_LINK");

		AssertClick(
			key_ddlDataDefinitionName = ${structureName},
			locator1 = "DDMSelectStructure#DDM_STRUCTURE_TABLE_NAME_LINK",
			value1 = ${structureName});

		SelectFrame(value1 = "relative=top");
	}

	@summary = "Default summary"
	macro gotoEntryMenuItem(entry = null, menuItem = null) {
		Click.waitForMenuToggleJSClick(
			key_webContentTitle = ${entry},
			locator1 = "WC#ENTRY_LIST_MENULIST_ICON");

		MenuItem.clickNoError(menuItem = ${menuItem});
	}

	@summary = "Default summary"
	macro gotoManageTemplatesViaStructures(structureName = null) {
		LexiconEntry.gotoEntryMenuItem(
			menuItem = "Manage Templates",
			rowEntry = ${structureName});

		SiteAdmin.viewPortletTitle(portletTitle = "Templates for Structure: ${structureName}");
	}

	@summary = "Default summary"
	macro gotoMoreStructuresViaPlusButton(pageMod = null) {
		LexiconEntry.gotoAdd();

		if (IsElementPresent(key_text = "More", locator1 = "Button#ANY")) {
			AssertElementPresent(
				key_caption = "Showing ${pageMod} Elements",
				locator1 = "Dropdown#CAPTION");
		}

		Button.clickMore();

		SelectFrame(locator1 = "IFrame#MODAL_BODY");
	}

	@summary = "Default summary"
	macro gotoPreviewEntryCP(webContentTitle = null, webContentTitleEscapeCharacter = null, liveSite = null) {
		WebContentNavigator._openDescriptiveMenulistCP(
			liveSite = ${liveSite},
			webContentTitle = ${webContentTitle},
			webContentTitleEscapeCharacter = ${webContentTitleEscapeCharacter});

		MenuItem.click(menuItem = "Preview");
	}

	@summary = "Default summary"
	macro gotoViewStatus(webContentTitle = null, webContentStatus = null) {
		var key_webContentTitle = ${webContentTitle};

		LexiconEntry.changeDisplayStyle(displayStyle = "list");

		LexiconEntry.viewActiveDisplayStyle(displayStyle = "list");

		AssertElementPresent(locator1 = "WC#ENTRY_LIST_THUMBNAIL");

		AssertTextEquals(
			locator1 = "WC#ENTRY_LIST_WORKFLOW_STATUS",
			value1 = ${webContentStatus});

		WebContentNavigator.gotoEditCP(webContentTitle = ${webContentTitle});
	}

	@summary = "Default summary"
	macro gotoViewTemplate(templateName = null) {
		AssertClick(
			key_ddlTemplateName = ${templateName},
			locator1 = "DDMSelectTemplate#TEMPLATE_TABLE_NAME_LINK",
			value1 = ${templateName});
	}

	@summary = "Default summary"
	macro openFolderInWebContentAdmin(depotName = null, baseURL = null, folderId = null) {
		var assetLibraryId = JSONGroupAPI._getDepotIdByName(depotName = ${depotName});

		Navigator.openWithAppendToBaseURL(
			baseURL = ${baseURL},
			urlAppend = "group/asset-library-${assetLibraryId}/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_folderId=${folderId}");
	}

	// You can use this navigation when you need to access the configuration quickly without checking the UI.

	@summary = "Default summary"
	macro openToAddBasicArticle(groupName = null, site = null, siteURLKey = null, folderName = null) {
		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		// Articles accessed in admin will stay in admin after publishing.

		var classNameId = JSONClassname._getClassNameId(value = "com.liferay.journal.model.JournalArticle");
		var globalGroupId = JSONGroupAPI._getGroupIdByName(
			groupName = "Global",
			site = "true");

		var basicWebContentStructureId = JSONDDMstructureAPI._getStructureId(
			classNameId = ${classNameId},
			groupId = ${globalGroupId},
			structureName = "Basic Web Content");

		if (isSet(folderName)) {
			var groupId = JSONWebcontentSetter.setGroupId(groupName = ${groupName});

			var folderId = JSONWebcontentSetter.setFolderId(
				folderName = ${folderName},
				groupId = ${groupId});

			Open(value1 = "${baseURL}/group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_mvcRenderCommandName=%2Fjournal%2Fedit_article&_com_liferay_journal_web_portlet_JournalPortlet_redirect=%2Fgroup%2F${siteURLKey}%2F~%2Fcontrol_panel%2Fmanage%3Fp_p_id%3Dcom_liferay_journal_web_portlet_JournalPortlet%26p_p_lifecycle%3D0%26p_p_state%3Dmaximized%26p_p_mode%3Dview%26_com_liferay_journal_web_portlet_JournalPortlet_displayStyle%3Ddescriptive%26_com_liferay_journal_web_portlet_JournalPortlet_folderId%3D${folderId}%26_com_liferay_journal_web_portlet_JournalPortlet_groupId%3D${groupId}%26p_p_auth%3DxwwxN5nQ&_com_liferay_journal_web_portlet_JournalPortlet_ddmStructureId=${basicWebContentStructureId}&_com_liferay_journal_web_portlet_JournalPortlet_folderId=${folderId}&_com_liferay_journal_web_portlet_JournalPortlet_groupId=${groupId}");
		}
		else {
			var groupId = JSONWebcontentSetter.setGroupId(
				groupName = ${groupName},
				site = ${site});

			if (!(isSet(folderId))) {
				var folderId = 0;
			}

			Open(value1 = "${baseURL}/group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_mvcRenderCommandName=%2Fjournal%2Fedit_article&_com_liferay_journal_web_portlet_JournalPortlet_redirect=%2Fgroup%2F${siteURLKey}%2F~%2Fcontrol_panel%2Fmanage%3Fp_p_id%3Dcom_liferay_journal_web_portlet_JournalPortlet%26p_p_lifecycle%3D0%26p_p_state%3Dmaximized%26p_v_l_s_g_id%3D${groupId}%26p_p_auth%3DxwwxN5nQ&_com_liferay_journal_web_portlet_JournalPortlet_ddmStructureId=${basicWebContentStructureId}&_com_liferay_journal_web_portlet_JournalPortlet_folderId=${folderId}&_com_liferay_journal_web_portlet_JournalPortlet_groupId=${groupId}");
		}
	}

	@summary = "Default summary"
	macro openToEditWCInSite(groupName = null, webContentTitle = null, urlTitle = null, siteURLKey = null, folderName = null) {
		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		if (!(isSet(site))) {
			var site = "true";
		}

		var groupId = JSONWebcontentSetter.setGroupId(
			groupName = ${groupName},
			site = ${site});
		var articleId = JSONWebcontentSetter.setArticleId(
			groupName = ${groupName},
			site = ${site},
			title = ${webContentTitle},
			urlTitle = ${urlTitle});

		if (isSet(folderName)) {
			var folderId = JSONWebcontentSetter.setFolderId(
				folderName = ${folderName},
				groupId = ${groupId});
		}
		else {
			var folderId = 0;
		}

		// Articles accessed in admin will stay in admin after editing.

		if (!(isSet(version))) {
			var version = "1.0";
		}

		if (isSet(staging)) {
			Navigator.openWithAppendToBaseURL(
				baseURL = ${baseURL},
				urlAppend = "group/${siteURLKey}-staging/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_mvcRenderCommandName=/journal/edit_article&_com_liferay_journal_web_portlet_JournalPortlet_redirect=${baseURL}/group/${siteURLKey}-staging/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_articleId=${articleId}&_com_liferay_journal_web_portlet_JournalPortlet_folderId=${folderId}&_com_liferay_journal_web_portlet_JournalPortlet_groupId=${groupId}&_com_liferay_journal_web_portlet_JournalPortlet_referringPortletResource=&_com_liferay_journal_web_portlet_JournalPortlet_version=${version}");
		}
		else {
			Navigator.openWithAppendToBaseURL(
				baseURL = ${baseURL},
				urlAppend = "group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_mvcRenderCommandName=/journal/edit_article&_com_liferay_journal_web_portlet_JournalPortlet_redirect=${baseURL}/group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_articleId=${articleId}&_com_liferay_journal_web_portlet_JournalPortlet_folderId=${folderId}&_com_liferay_journal_web_portlet_JournalPortlet_groupId=${groupId}&_com_liferay_journal_web_portlet_JournalPortlet_referringPortletResource=&_com_liferay_journal_web_portlet_JournalPortlet_version=${version}");
		}
	}

	@summary = "Default summary"
	macro openWebContentAdmin(baseURL = null, siteURLKey = null) {
		Navigator.openWithAppendToBaseURL(
			baseURL = ${baseURL},
			urlAppend = "group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet");
	}

	@summary = "Default summary"
	macro openWebContentStructuresAdmin(depotName = null, baseURL = null, siteURLKey = null) {
		if (isSet(depotName)) {
			var depotId = JSONGroupAPI._getDepotIdByName(depotName = ${depotName});

			var urlKey = "asset-library-${depotId}";
		}
		else {
			var urlKey = ${siteURLKey};
		}

		Navigator.openWithAppendToBaseURL(
			baseURL = ${baseURL},
			urlAppend = "group/${urlKey}/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_mvcPath=%2Fview_ddm_structures.jsp&p_p_auth=sNIcMAzl");
	}

	@summary = "Default summary"
	macro openWebContentTemplatesAdmin(baseURL = null, siteURLKey = null) {
		Navigator.openWithAppendToBaseURL(
			baseURL = ${baseURL},
			urlAppend = "group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_journal_web_portlet_JournalPortlet&_com_liferay_journal_web_portlet_JournalPortlet_mvcPath=%2Fview_ddm_templates.jsp");
	}

}