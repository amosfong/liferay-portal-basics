definition {

	@summary = "Default summary"
	macro addCSSURLRow() {
		Click(
			key_id = "cssURLs",
			locator1 = "ClientExtensionEntry#ADD_URL_ROW");
	}

	@summary = "Default summary"
	macro addCustomElement(entryName = null, entryURL = null, entryHtmlName = null) {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = ${entryName});

		Type(
			key_text = "HTML Element Name",
			locator1 = "TextInput#ANY",
			value1 = ${entryHtmlName});

		Type(
			key_id = "ClientExtensionAdminPortlet_url",
			locator1 = "ClientExtensionEntry#URL",
			value1 = ${entryURL});
	}

	@summary = "Default summary"
	macro addCustomElementAllFields(entryProperties = null, entryName = null, entryURL = null, entryHtmlName = null, entryCssurl = null) {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = ${entryName});

		Type(
			key_text = "HTML Element Name",
			locator1 = "TextInput#ANY",
			value1 = ${entryHtmlName});

		Type(
			key_id = "ClientExtensionAdminPortlet_url",
			locator1 = "ClientExtensionEntry#URL",
			value1 = ${entryURL});

		Type(
			key_text = "CSS URL",
			locator1 = "TextInput#ANY",
			value1 = ${entryCssurl});

		Type(
			locator1 = "ClientExtensionEntry#REMOTE_APPS_PROPERTIES",
			value1 = ${entryProperties});
	}

	@summary = "Default summary"
	macro addCustomElementMultipleURLs(entryCssurl2 = null, entryName = null, entryCssurl1 = null, entryUrl2 = null, entryUrl1 = null, entryHtmlName = null) {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = ${entryName});

		Type(
			key_text = "HTML Element Name",
			locator1 = "TextInput#ANY",
			value1 = ${entryHtmlName});

		ClientExtensionEntry.addURLRow();

		ClientExtensionEntry.addCSSURLRow();

		Type(
			key_id = "ClientExtensionAdminPortlet_url",
			key_index = 1,
			locator1 = "ClientExtensionEntry#URL_ROW_INDEX",
			value1 = ${entryUrl1});

		Type(
			key_id = "ClientExtensionAdminPortlet_url",
			key_index = 2,
			locator1 = "ClientExtensionEntry#URL_ROW_INDEX",
			value1 = ${entryUrl2});

		Type(
			key_id = "cssURLs",
			key_index = 1,
			locator1 = "ClientExtensionEntry#URL_ROW_INDEX",
			value1 = ${entryCssurl1});

		Type(
			key_id = "cssURLs",
			key_index = 2,
			locator1 = "ClientExtensionEntry#URL_ROW_INDEX",
			value1 = ${entryCssurl2});
	}

	@summary = "Default summary"
	macro addEntry(entryName = null, entryURL = null, mainURL = null, entryHTMLElementName = null) {
		Type(
			key_text = "Name",
			locator1 = "TextInput#ANY",
			value1 = ${entryName});

		if (isSet(mainURL)) {
			Type(
				key_id = "ClientExtensionAdminPortlet_mainURL",
				locator1 = "ClientExtensionEntry#URL",
				value1 = ${mainURL});
		}
		else {
			Type(
				key_id = "ClientExtensionAdminPortlet_url",
				locator1 = "ClientExtensionEntry#URL",
				value1 = ${entryURL});
		}

		if (isSet(entryHTMLElementName)) {
			Type(
				key_id = "ClientExtensionAdminPortlet_htmlElementName",
				locator1 = "ClientExtensionEntry#URL",
				value1 = ${entryHTMLElementName});
		}
	}

	@summary = "Default summary"
	macro addPortletCategory(categoryName = null) {
		Select(
			locator1 = "ClientExtensionEntry#REMOTE_APPS_PORTLET_CATEGORY_NAME",
			value1 = ${categoryName});
	}

	@summary = "Default summary"
	macro addURLRow() {
		Click(
			key_id = "ClientExtensionAdminPortlet__urls",
			locator1 = "ClientExtensionEntry#ADD_URL_ROW");
	}

	@summary = "Default summary"
	macro removeURLRow(key_id = null) {
		Click(
			key_id = ${key_id},
			locator1 = "ClientExtensionEntry#REMOVE_URL_ROW");
	}

}