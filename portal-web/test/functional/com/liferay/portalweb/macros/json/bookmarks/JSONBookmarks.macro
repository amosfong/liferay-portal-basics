definition {

	@summary = "Default summary"
	macro addBookmarks(groupName = null, site = null, assetCategoryIds = null, name = null, assetTagNames = null, description = null, folderName = null, workflowAction = null) {
		Variables.assertDefined(parameterList = "${groupName},${name},${url}");

		var groupId = JSONBookmarksSetter.setGroupId(
			groupName = ${groupName},
			site = ${site});

		var folderId = JSONBookmarksSetter.setFolderId(
			folderName = ${folderName},
			groupId = ${groupId});
		var url = StringUtil.replace(${url}, ":", "%3A");
		var serviceContext = JSONBookmarksSetter.setServiceContext(
			assetCategoryIds = ${assetCategoryIds},
			assetTagNames = ${assetTagNames},
			workflowAction = ${workflowAction});

		JSONBookmarksAPI._addBookmarks(
			description = ${description},
			folderId = ${folderId},
			groupId = ${groupId},
			name = ${name},
			serviceContext = ${serviceContext},
			url = ${url});
	}

	@summary = "Default summary"
	macro addFolder(parentFolderName = null, groupName = null, site = null, assetCategoryIds = null, folderDescription = null, assetTagNames = null, folderName = null, workflowAction = null) {
		Variables.assertDefined(parameterList = "${folderName},${groupName}");

		var groupId = JSONBookmarksSetter.setGroupId(
			groupName = ${groupName},
			site = ${site});

		var parentFolderId = JSONBookmarksSetter.setFolderId(
			folderName = ${parentFolderName},
			groupId = ${groupId});

		if (${groupName} == "Guest") {
			var serviceContext = JSONBookmarksSetter.setServiceContext(
				assetCategoryIds = ${assetCategoryIds},
				assetTagNames = ${assetTagNames},
				groupName = "Guest",
				workflowAction = ${workflowAction});
		}
		else {
			var serviceContext = JSONBookmarksSetter.setServiceContext(
				assetCategoryIds = ${assetCategoryIds},
				assetTagNames = ${assetTagNames},
				groupName = ${groupName},
				site = ${site},
				workflowAction = ${workflowAction});
		}

		JSONBookmarksAPI._addFolder(
			folderDescription = ${folderDescription},
			folderName = ${folderName},
			parentFolderId = ${parentFolderId},
			serviceContext = ${serviceContext});
	}

}