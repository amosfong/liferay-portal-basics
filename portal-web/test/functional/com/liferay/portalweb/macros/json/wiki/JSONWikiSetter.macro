definition {

	@summary = "Default summary"
	macro setGroupId(groupName = null) {
		Variables.assertDefined(parameterList = ${groupName});

		if (!(isSet(site))) {
			var site = "true";
		}

		var groupId = JSONGroupAPI._getGroupIdByName(
			groupName = ${groupName},
			site = ${site});

		return ${groupId};
	}

	@summary = "Default summary"
	macro setNodeId(noSelenium = null, groupName = null, name = null) {
		Variables.assertDefined(parameterList = "${name},${groupName}");

		if (!(isSet(site))) {
			var site = "true";
		}

		var groupId = JSONGroupAPI._getGroupIdByName(
			groupName = ${groupName},
			noSelenium = ${noSelenium},
			site = ${site});

		var nodeId = JSONWikiAPI._getNodeIdByName(
			groupId = ${groupId},
			name = ${name});

		return ${nodeId};
	}

	@summary = "Default summary"
	macro setPageId(groupName = null, name = null, title = null) {
		Variables.assertDefined(parameterList = "${title},${groupName},${name}");

		if (!(isSet(site))) {
			var site = "true";
		}

		var groupId = JSONGroupAPI._getGroupIdByName(
			groupName = ${groupName},
			site = ${site});

		var nodeId = JSONWikiAPI._getNodeIdByName(
			groupId = ${groupId},
			name = ${name});

		var pageId = JSONWikiAPI._getPageIdByName(
			nodeId = ${nodeId},
			title = ${title});

		return ${pageId};
	}

	@summary = "Default summary"
	macro setResourcePrimKey(groupName = null, name = null, title = null) {
		Variables.assertDefined(parameterList = "${title},${groupName},${name}");

		if (!(isSet(site))) {
			var site = "true";
		}

		var groupId = JSONGroupAPI._getGroupIdByName(
			groupName = ${groupName},
			site = ${site});

		var nodeId = JSONWikiAPI._getNodeIdByName(
			groupId = ${groupId},
			name = ${name});

		var resourcePrimKey = JSONWikiAPI._getPageResourcePrimKey(
			nodeId = ${nodeId},
			title = ${title});

		return ${resourcePrimKey};
	}

	@summary = "Default summary"
	macro setServiceContext(groupName = null, assetCategoryIds = null, assetTagNames = null) {
		var serviceContext = JSONServiceContextUtil.setServiceContext(
			assetCategoryIds = ${assetCategoryIds},
			assetTagNames = ${assetTagNames},
			groupName = ${groupName});

		return ${serviceContext};
	}

}