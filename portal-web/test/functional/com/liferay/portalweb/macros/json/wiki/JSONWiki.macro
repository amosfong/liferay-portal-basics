definition {

	@summary = "Default summary"
	macro addAttachmentToWikiPage(wikiNodeName = null, groupName = null, fileName = null, site = null, mimeType = null, wikiPageName = null) {
		Variables.assertDefined(parameterList = "${groupName},${wikiNodeName},${wikiPageName},${fileName},${mimeType}");

		var nodeId = JSONWikiSetter.setNodeId(
			groupName = ${groupName},
			name = ${wikiNodeName},
			site = ${site});

		JSONWikiAPI._addAttachmentToWikiPage(
			fileName = ${fileName},
			mimeType = ${mimeType},
			nodeId = ${nodeId},
			title = ${wikiPageName});
	}

	@summary = "Default summary"
	macro addWikiChildPage(wikiNodeName = null, groupName = null, site = null, assetCategoryIds = null, viewableBy = null, wikiChildPageName = null, encodingFormat = null, wikiParentPageName = null, tagName = null, wikiChildPageContent = null) {
		Variables.assertDefined(parameterList = "${wikiParentPageName},${wikiChildPageName},${wikiNodeName}");

		var parentResourcePrimKey = JSONWikiSetter.setResourcePrimKey(
			groupName = ${groupName},
			name = ${wikiNodeName},
			site = ${site},
			title = ${wikiParentPageName});

		JSONWikiAPI._addWikiChildPage(
			assetCategoryIds = ${assetCategoryIds},
			content = ${wikiChildPageContent},
			encodingFormat = ${encodingFormat},
			headline = ${wikiChildPageName},
			parentResourcePrimKey = ${parentResourcePrimKey},
			tagName = ${tagName},
			viewableBy = ${viewableBy});
	}

	@summary = "Default summary"
	macro addWikiNode(wikiNodeName = null, groupName = null, wikiNodeDescription = null) {
		Variables.assertDefined(parameterList = "${groupName},${wikiNodeName}");

		var serviceContext = JSONWikiSetter.setServiceContext(groupName = ${groupName});

		JSONWikiAPI._addWikiNode(
			description = ${wikiNodeDescription},
			name = ${wikiNodeName},
			serviceContext = ${serviceContext});
	}

	@summary = "Default summary"
	macro addWikiPage(wikiNodeName = null, noSelenium = null, groupName = null, site = null, assetCategoryIds = null, viewableBy = null, encodingFormat = null, wikiPageContent = null, wikiPageName = null, tagName = null) {
		Variables.assertDefined(parameterList = "${groupName},${wikiNodeName},${wikiPageName}");

		var nodeId = JSONWikiSetter.setNodeId(
			groupName = ${groupName},
			name = ${wikiNodeName},
			noSelenium = ${noSelenium},
			site = ${site});

		var response = JSONWikiAPI._addWikiPage(
			assetCategoryIds = ${assetCategoryIds},
			content = ${wikiPageContent},
			encodingFormat = ${encodingFormat},
			headline = ${wikiPageName},
			nodeId = ${nodeId},
			tagName = ${tagName},
			viewableBy = ${viewableBy});

		return ${response};
	}

	@summary = "Default summary"
	macro deleteWikiPageById(wikiPageId = null) {
		Variables.assertDefined(parameterList = ${wikiPageId});

		var response = JSONWikiAPI._deleteWikiPageById(wikiPageId = ${wikiPageId});

		return ${response};
	}

	@summary = "Default summary"
	macro getWikiPageExternalReferenceCodeByWikiPageId(wikiPageId = null) {
		Variables.assertDefined(parameterList = ${wikiPageId});

		var response = JSONWikiAPI._getWikiPageById(wikiPageId = ${wikiPageId});

		var wikiPageExternalReferenceCode = JSONPathUtil.getErcValue(response = ${response});

		return ${wikiPageExternalReferenceCode};
	}

	@summary = "Default summary"
	macro movePageToTrash(wikiNodeName = null, groupName = null, site = null, wikiPageName = null) {
		Variables.assertDefined(parameterList = "${groupName},${wikiNodeName},${wikiPageName}");

		var nodeId = JSONWikiSetter.setNodeId(
			groupName = ${groupName},
			name = ${wikiNodeName},
			site = ${site});

		JSONWikiAPI._movePageToTrash(
			nodeId = ${nodeId},
			title = ${wikiPageName});
	}

	@summary = "Default summary"
	macro updateWikiPage(wikiNodeName = null, groupName = null, site = null, assetCategoryIds = null, viewableBy = null, encodingFormat = null, wikiPageContent = null, wikiPageName = null, tagName = null) {
		Variables.assertDefined(parameterList = "${groupName},${wikiNodeName},${wikiPageName}");

		var resourcePrimKey = JSONWikiSetter.setResourcePrimKey(
			groupName = ${groupName},
			name = ${wikiNodeName},
			site = ${site},
			title = ${wikiPageName});

		JSONWikiAPI._updateWikiPage(
			assetCategoryIds = ${assetCategoryIds},
			content = ${wikiPageContent},
			encodingFormat = ${encodingFormat},
			headline = ${wikiPageName},
			resourcePrimKey = ${resourcePrimKey},
			tagName = ${tagName},
			viewableBy = ${viewableBy});
	}

}