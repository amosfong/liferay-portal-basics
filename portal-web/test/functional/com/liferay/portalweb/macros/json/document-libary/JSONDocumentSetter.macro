definition {

	@summary = "Default summary"
	macro setServiceContext(assetCategoryIds = null, addGroupPermissions = null, addGuestPermissions = null, assetTagNames = null, workflowAction = null) {
		if (!(isSet(serviceContext))) {
			var serviceContext = JSONServiceContextUtil.setServiceContext(
				addGroupPermissions = ${addGroupPermissions},
				addGuestPermissions = ${addGuestPermissions},
				assetCategoryIds = ${assetCategoryIds},
				assetTagNames = ${assetTagNames},
				workflowAction = ${workflowAction});
		}

		return ${serviceContext};
	}

}