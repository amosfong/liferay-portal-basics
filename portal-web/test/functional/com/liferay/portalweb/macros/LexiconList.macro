definition {

	@summary = "Check the checkbox of a list entry"
	macro checkListCheckbox(listEntry = null) {
		Check(
			key_listEntry = ${listEntry},
			locator1 = "LexiconList#LIST_ENTRY_CHECKBOX");
	}

	@summary = "Default summary"
	macro clickEllipsis(userRole = null, listEntry = null) {
		if (${userRole} == "Guest") {
			Click(
				key_listEntry = ${listEntry},
				locator1 = "LexiconList#LIST_ENTRY_ELLIPSIS");
		}
		else {
			Click.waitForMenuToggleJSClick(
				key_listEntry = ${listEntry},
				locator1 = "LexiconList#LIST_ENTRY_ELLIPSIS");
		}
	}

	@summary = "Default summary"
	macro clickEllipsisItem(item = null, userRole = null, listEntry = null) {
		LexiconList.clickEllipsis(
			listEntry = ${listEntry},
			userRole = ${userRole});

		MenuItem.clickNoError(menuItem = ${item});
	}

	@summary = "Go to location by clicking on the breadcrumb item in the entry"
	macro clickListEntryBreadcrumbItem(listEntry = null) {
		Click(
			key_breadcrumbName = ${breadcrumbName},
			key_listEntry = ${listEntry},
			locator1 = "LexiconList#LIST_ENTRY_BREADCRUMB_ITEM");
	}

	@summary = "Default summary"
	macro clickListEntryTitle(listEntry = null) {
		Click(
			key_listEntry = ${listEntry},
			locator1 = "LexiconList#LIST_ENTRY_TITLE_LINK");
	}

	@summary = "Default summary"
	macro delete(listEntry = null) {
		LexiconList.clickEllipsisItem(
			item = "Delete",
			listEntry = ${listEntry});

		Confirm(value1 = "null");
	}

	@summary = "Default summary"
	macro viewAnyList(listEntry = null) {
		AssertTextEquals.assertPartialText(
			key_listEntry = ${listEntry},
			locator1 = "LexiconList#ANY_LIST",
			value1 = ${listEntry});
	}

	@summary = "Default summary"
	macro viewListEntryLabel(listEntryLabel = null, listEntry = null) {
		AssertTextEquals(
			key_listEntry = ${listEntry},
			key_listEntryLabel = ${listEntryLabel},
			locator1 = "LexiconList#LIST_ENTRY_LABEL",
			value1 = ${listEntryLabel});
	}

	@summary = "Default summary"
	macro viewListEntryNotPresent(listEntry = null) {
		AssertElementNotPresent(
			key_listEntry = ${listEntry},
			locator1 = "LexiconList#LIST_ENTRY");
	}

	@summary = "Default summary"
	macro viewListEntryPresent(listEntry = null) {
		AssertVisible(
			key_listEntry = ${listEntry},
			locator1 = "LexiconList#LIST_ENTRY");
	}

	@summary = "Default summary"
	macro viewListEntrySpecific(position = null, listEntry = null) {
		AssertTextEquals.assertPartialText(
			key_listEntry = ${listEntry},
			key_position = ${position},
			locator1 = "LexiconList#LIST_ENTRY_SPECIFIC",
			value1 = ${listEntry});
	}

	@summary = "Default summary"
	macro viewListEntrySubtitle(listEntrySubtitle = null, listEntry = null) {
		AssertTextEquals(
			key_listEntry = ${listEntry},
			key_listEntrySubtitle = ${listEntrySubtitle},
			locator1 = "LexiconList#LIST_ENTRY_SUBTITLE",
			value1 = ${listEntrySubtitle});
	}

	@summary = "Default summary"
	macro viewListEntryTitle(listEntry = null) {
		AssertTextEquals(
			key_listEntry = ${listEntry},
			locator1 = "LexiconList#LIST_ENTRY_TITLE",
			value1 = ${listEntry});
	}

	@summary = "Default summary"
	macro viewListEntryTitleLink(listEntry = null) {
		AssertTextEquals(
			key_listEntry = ${listEntry},
			locator1 = "LexiconList#LIST_ENTRY_TITLE_LINK",
			value1 = ${listEntry});
	}

	@summary = "Default summary"
	macro viewListEntryTitleLinkSpecific(position = null, listEntry = null) {
		AssertTextEquals.assertPartialText(
			key_listEntry = ${listEntry},
			key_position = ${position},
			locator1 = "LexiconList#LIST_ENTRY_TITLE_LINK_SPECIFIC",
			value1 = ${listEntry});
	}

}