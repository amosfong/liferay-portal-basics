	@summary = "Default summary"
	macro viewParentOrganizationCP(orgName = null, subOrgName = null, parentOrgName = null) {
		Organization.gotoEdit(orgName = ${orgName});

		var key_orgName = ${subOrgName};

		Click(locator1 = "UsersAndOrganizationsEditOrganization#PARENT_ORGANIZATION_SELECT_BUTTON");

		SelectFrame(
			key_title = "Select Parent Organization",
			locator1 = "IFrame#MODAL_ANY");

		AssertTextEquals(
			key_columnName = "name",
			key_rowEntry = ${subOrgName},
			locator1 = "ContentRow#ENTRY_CONTENT_ROW_NUMBER_TD",
			value1 = ${subOrgName});

		AssertTextEquals(
			key_columnName = "path",
			key_rowEntry = ${subOrgName},
			locator1 = "ContentRow#ENTRY_CONTENT_ROW_NUMBER_TD",
			value1 = ${parentOrgName});

		IFrame.closeFrame();
	}

	@summary = "Default summary"
	macro viewSuborganizationCP(orgType = null, orgName = null, suborgType = null, suborgName = null) {
		LexiconEntry.openEntryMenu(rowEntry = ${suborgName});

		MenuItem.click(menuItem = "Edit");

		var key_orgType = ${orgType};

		AssertTextEquals(
			locator1 = "Portlet#HEADER",
			value1 = "Edit ${suborgName}");

		AssertTextEquals(
			locator1 = "TextInput#NAME",
			value1 = ${suborgName});

		AssertTextEquals.assertValue(
			locator1 = "UsersAndOrganizationsEditOrganization#DETAILS_TYPE",
			value1 = ${suborgType});

		AssertTextEquals(
			locator1 = "UsersAndOrganizationsEditOrganization#PARENT_ORGANIZATION_NAME",
			value1 = ${orgName});
	}

	@summary = "Default summary"
	macro viewUser(userName = null) {
		LexiconEntry.viewEntryName(rowEntry = ${userName});
	}

}