	@summary = "Default summary"
	macro gotoWidgetSpecificTabByURL(wikiNodeName = null, layoutURLKey = null, baseURL = null, siteURLKey = null, tabURLKey = null) {
		Navigator.openWithAppendToBaseURL(
			baseURL = ${baseURL},
			urlAppend = "web/${siteURLKey}/${layoutURLKey}/-/wiki/${wikiNodeName}/${tabURLKey}?wiki_nodeName=${wikiNodeName}");
	}

	// You can use this navigation when you need to access the configuration quickly without checking the UI.

	@summary = "Default summary"
	macro openToAddChildPage(wikiNodeName = null, layoutURLKey = null, groupName = null, siteURLKey = null, wikiParentPageTitle = null) {
		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		var nodeId = JSONWikiSetter.setNodeId(
			groupName = ${groupName},
			name = ${wikiNodeName});

		if (isSet(layoutURLKey)) {
			Open(value1 = "${baseURL}/web/${siteURLKey}/${layoutURLKey}?p_p_id=com_liferay_wiki_web_portlet_WikiPortlet&_com_liferay_wiki_web_portlet_WikiPortlet_mvcRenderCommandName=%2Fwiki%2Fedit_page&_com_liferay_wiki_web_portlet_WikiPortlet_editTitle=1&p_r_p_http%3A%2F%2Fwww.liferay.com%2Fpublic-render-parameters%2Fwiki_nodeId=${nodeId}&_com_liferay_wiki_web_portlet_WikiPortlet_parentTitle=${wikiParentPageTitle}");
		}
		else {
			Open(value1 = "${baseURL}/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_wiki_web_portlet_WikiAdminPortlet&_com_liferay_wiki_web_portlet_WikiAdminPortlet_mvcRenderCommandName=%2Fwiki%2Fedit_page&_com_liferay_wiki_web_portlet_WikiAdminPortlet_editTitle=1&_com_liferay_wiki_web_portlet_WikiAdminPortlet_nodeId=${nodeId}&_com_liferay_wiki_web_portlet_WikiAdminPortlet_parentTitle=${wikiParentPageTitle}");
		}
	}

	@summary = "Default summary"
	macro openToAddPage(wikiNodeName = null, layoutURLKey = null, groupName = null, siteURLKey = null) {
		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		var nodeId = JSONWikiSetter.setNodeId(
			groupName = ${groupName},
			name = ${wikiNodeName});

		// Pages accessed in site pages will be redirected back to the page after publishing. Those accessed in admin will stay in admin after publishing.

		if (isSet(layoutURLKey)) {
			Open(value1 = "${baseURL}/web/guest/${layoutURLKey}?p_p_id=com_liferay_wiki_web_portlet_WikiPortlet&_com_liferay_wiki_web_portlet_WikiPortlet_mvcRenderCommandName=%2Fwiki%2Fedit_page&_com_liferay_wiki_web_portlet_WikiPortlet_editTitle=1&p_r_p_http%3A%2F%2Fwww.liferay.com%2Fpublic-render-parameters%2Fwiki_nodeId=${nodeId}&p_r_p_http%3A%2F%2Fwww.liferay.com%2Fpublic-render-parameters%2Fwiki_title=");
		}
		else {
			Open(value1 = "${baseURL}/group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_wiki_web_portlet_WikiAdminPortlet&_com_liferay_wiki_web_portlet_WikiAdminPortlet_mvcRenderCommandName=%2Fwiki%2Fedit_page&_com_liferay_wiki_web_portlet_WikiAdminPortlet_redirect=&_com_liferay_wiki_web_portlet_WikiAdminPortlet_editTitle=1&_com_liferay_wiki_web_portlet_WikiAdminPortlet_nodeId=${nodeId}");
		}
	}

	@summary = "Default summary"
	macro openToEditPage(wikiNodeName = null, layoutURLKey = null, groupName = null, baseURL = null, wikiPageTitle = null, siteURLKey = null) {
		var nodeId = JSONWikiSetter.setNodeId(
			groupName = ${groupName},
			name = ${wikiNodeName});

		if (isSet(layoutURLKey)) {
			Navigator.openWithAppendToBaseURL(
				baseURL = ${baseURL},
				urlAppend = "web/${siteURLKey}/${layoutURLKey}?p_p_id=com_liferay_wiki_web_portlet_WikiPortlet&_com_liferay_wiki_web_portlet_WikiPortlet_mvcRenderCommandName=%2Fwiki%2Fedit_page&p_r_p_http%3A%2F%2Fwww.liferay.com%2Fpublic-render-parameters%2Fwiki_nodeId=${nodeId}&p_r_p_http%3A%2F%2Fwww.liferay.com%2Fpublic-render-parameters%2Fwiki_title=${wikiPageTitle}");
		}
		else {
			Navigator.openWithAppendToBaseURL(
				baseURL = ${baseURL},
				urlAppend = "group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_wiki_web_portlet_WikiAdminPortlet&_com_liferay_wiki_web_portlet_WikiAdminPortlet_mvcRenderCommandName=%2Fwiki%2Fedit_page&_com_liferay_wiki_web_portlet_WikiAdminPortlet_nodeId=${wikiNodeName}&_com_liferay_wiki_web_portlet_WikiAdminPortlet_title=${wikiPageTitle}");
		}
	}

	@summary = "Default summary"
	macro openToWikiNode(wikiNodeName = null, groupName = null, baseURL = null, siteURLKey = null) {
		var nodeId = JSONWikiSetter.setNodeId(
			groupName = ${groupName},
			name = ${wikiNodeName});

		Navigator.openWithAppendToBaseURL(
			baseURL = ${baseURL},
			urlAppend = "group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_wiki_web_portlet_WikiAdminPortlet&_com_liferay_wiki_web_portlet_WikiAdminPortlet_mvcRenderCommandName=%2Fwiki%2Fview_pages&_com_liferay_wiki_web_portlet_WikiAdminPortlet_navigation=all-pages&_com_liferay_wiki_web_portlet_WikiAdminPortlet_nodeId=${nodeId}");
	}

	@summary = "Default summary"
	macro openToWikiPage(wikiNodeName = null, layoutURLKey = null, baseURL = null, wikiPageTitle = null, siteURLKey = null) {
		if (isSet(layoutURLKey)) {
			Navigator.openWithAppendToBaseURL(
				baseURL = ${baseURL},
				urlAppend = "web/${siteURLKey}/${layoutURLKey}/-/wiki/${wikiNodeName}/${wikiPageTitle}?wiki_title=${wikiPageTitle}");
		}
		else {
			Navigator.openWithAppendToBaseURL(
				baseURL = ${baseURL},
				urlAppend = "group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_wiki_web_portlet_WikiAdminPortlet&_com_liferay_wiki_web_portlet_WikiAdminPortlet_mvcRenderCommandName=%2Fwiki%2Fview&_com_liferay_wiki_web_portlet_WikiAdminPortlet_nodeName=${wikiNodeName}&_com_liferay_wiki_web_portlet_WikiAdminPortlet_title=${wikiPageTitle}");
		}
	}

	@summary = "Default summary"
	macro openWikiAdmin(siteURLKey = null) {
		Navigator.openWithAppendToBaseURL(urlAppend = "group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_wiki_web_portlet_WikiAdminPortlet");
	}

