definition {

	@summary = "Default summary"
	macro click(menuItem = null) {
		Click(
			key_menuItem = ${menuItem},
			locator1 = "MenuItem#DROPDOWN_MENU_ITEM");
	}

	@summary = "Default summary"
	macro viewDisabledItem(itemList = null) {
		for (var item : list ${itemList}) {
			AssertElementPresent(
				key_text = ${item},
				locator1 = "Dropdown#ANY_DISABLED");
		}
	}

	@summary = "Default summary"
	macro viewIcon(iconList = null) {
		for (var icon : list ${iconList}) {
			AssertVisible(
				key_text = ${icon},
				locator1 = "MenuItem#ANY_ICON");
		}
	}

}