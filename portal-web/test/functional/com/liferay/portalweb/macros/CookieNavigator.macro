definition {

	@description = "Go to Instance settings"
	@summary = "Default summary"
	macro gotoInstanceSettings(portlet = null) {
		Variables.assertDefined(parameterList = ${portlet});

		var baseURL = ${baseURL};
		var portletId = "";

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		if (${portlet} == "Banner") {
			var portletId = "com.liferay.cookies.configuration.banner.CookiesBannerConfiguration";
		}

		if (${portlet} == "Consent Panel") {
			var portletId = "com.liferay.cookies.configuration.consent.CookiesConsentConfiguration";
		}

		if (${portlet} == "Preference Handling") {
			var portletId = "com.liferay.cookies.configuration.CookiesPreferenceHandlingConfiguration";
		}

		Open(value1 = "${baseURL}/group/control_panel/manage?p_p_id=com_liferay_configuration_admin_web_portlet_InstanceSettingsPortlet&_com_liferay_configuration_admin_web_portlet_InstanceSettingsPortlet_factoryPid=${portletId}&_com_liferay_configuration_admin_web_portlet_InstanceSettingsPortlet_mvcRenderCommandName=%2Fconfiguration_admin%2Fedit_configuration");

		takeScreenshot();
	}

	@description = "Go to Site settings"
	@summary = "Default summary"
	macro gotoSiteSettings(portlet = null) {
		Variables.assertDefined(parameterList = ${portlet});

		takeScreenshot();

		var baseURL = ${baseURL};
		var siteURLKey = ${siteURLKey};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		if (!(isSet(siteURLKey))) {
			var siteURLKey = "guest";
		}

		if (${portlet} == "Banner") {
			var portletId = "com.liferay.cookies.configuration.banner.CookiesBannerConfiguration";
		}

		if (${portlet} == "Consent Panel") {
			var portletId = "com.liferay.cookies.configuration.consent.CookiesConsentConfiguration";
		}

		if (${portlet} == "Preference Handling") {
			var portletId = "com.liferay.cookies.configuration.CookiesPreferenceHandlingConfiguration";
		}

		Open(value1 = "${baseURL}/group/${siteURLKey}/~/control_panel/manage/-/site/settings?_com_liferay_site_admin_web_portlet_SiteSettingsPortlet_factoryPid=${portletId}&_com_liferay_site_admin_web_portlet_SiteSettingsPortlet_mvcRenderCommandName=%2Fconfiguration_admin%2Fedit_configuration");

		takeScreenshot();
	}

	@description = "Go to System settings"
	@summary = "Default summary"
	macro gotoSystemSettings(portlet = null) {
		Variables.assertDefined(parameterList = ${portlet});

		var baseURL = ${baseURL};
		var portletId = "";

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		if (${portlet} == "Banner") {
			var portletId = "com.liferay.cookies.configuration.banner.CookiesBannerConfiguration";
		}

		if (${portlet} == "Consent Panel") {
			var portletId = "com.liferay.cookies.configuration.consent.CookiesConsentConfiguration";
		}

		if (${portlet} == "Preference Handling") {
			var portletId = "com.liferay.cookies.configuration.CookiesPreferenceHandlingConfiguration";
		}

		Open(value1 = "${baseURL}/group/control_panel/manage?p_p_id=com_liferay_configuration_admin_web_portlet_SystemSettingsPortlet&p_p_lifecycle=0&p_p_state=maximized&p_p_mode=view&_com_liferay_configuration_admin_web_portlet_SystemSettingsPortlet_factoryPid=${portletId}&_com_liferay_configuration_admin_web_portlet_SystemSettingsPortlet_mvcRenderCommandName=%2Fconfiguration_admin%2Fedit_configuration");

		takeScreenshot();
	}

}