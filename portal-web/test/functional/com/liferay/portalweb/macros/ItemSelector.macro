	macro viewWCCurrentSiteFilterResults(webContentTitle = null, noWebContentTitle = null, viewNoLocationInfo = null) {
		LexiconEntry.changeDisplayStyle(displayStyle = "list");

		AssertVisible(
			key_rowEntry = ${webContentTitle},
