definition {

	@summary = "Default summary"
	macro resizeImage(dimension = null) {
		Select(
			locator1 = "Select#IMAGE_RATIO",
			value1 = ${dimension});
	}

	@summary = "Default summary"
	macro rotateImage() {
		Click(
			key_text = "rotate",
			locator1 = "Icon#ANY");
	}

	@summary = "Default summary"
	macro saveChanges() {
		Button.clickSave();

		SelectFrameTop();
	}

}