	@summary = "Default summary"
	macro openSiteStagingConfiguration(siteName = null) {
		var siteNameURL = StringUtil.replace(${siteName}, " ", "-");

		var siteNameURL = StringUtil.lowerCase(${siteNameURL});
		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		Navigator.openWithAppendToBaseURL(urlAppend = "group/${siteNameURL}-staging/~/control_panel/manage?p_p_id=com_liferay_staging_processes_web_portlet_StagingProcessesPortlet");
	}

	@summary = "Default summary"
	macro openSiteURL(siteName = null) {
		var siteNameURL = StringUtil.replace(${siteName}, " ", "-");

		var siteNameURL = StringUtil.lowerCase(${siteNameURL});
		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		Open(value1 = "${baseURL}/web/${siteNameURL}");
	}

	@summary = "Default summary"
	macro openSpecificURL(domain = null, url = null) {
		Open(value1 = ${url});

		Alert.confirmSecurity(domain = ${domain});
	}

	@summary = "Default summary"
	macro openStagingSiteURL(siteName = null) {
		var siteNameURL = StringUtil.replace(${siteName}, " ", "-");

		var siteNameURL = StringUtil.lowerCase(${siteNameURL});
		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		if (isSet(defaultSite)) {
			Open(value1 = "${baseURL}/web/guest-staging");
		}
		else {
			Open(value1 = "${baseURL}/web/${siteNameURL}-staging");
		}
	}

	@summary = "Default summary"
	macro openStoreURL() {
		Navigator.openWithAppendToBaseURL(urlAppend = "group/control_panel/manage?p_p_id=com_liferay_marketplace_store_web_portlet_MarketplaceStorePortlet");
	}

	@summary = "Default summary"
	macro openToGroupPagesPortlet(groupName = null, site = null, portalURL = null, siteURLKey = null, portlet = null, layoutName = null) {
		var plid = JSONLayout.getPlid(
			groupName = ${groupName},
			layoutName = ${layoutName},
			portalURL = ${portalURL},
			site = ${site});

		if (${portlet} == "General") {
			var screenNavigationEntryKey = "general";
		}

		if (${portlet} == "Open Graph") {
			var screenNavigationEntryKey = "open-graph";
		}

		if (${portlet} == "SEO") {
			var screenNavigationEntryKey = "seo";
		}

		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		Open(value1 = "${baseURL}/group/${siteURLKey}/~/control_panel/manage?p_p_id=com_liferay_layout_admin_web_portlet_GroupPagesPortlet&_com_liferay_layout_admin_web_portlet_GroupPagesPortlet_tabs1=pages&_com_liferay_layout_admin_web_portlet_GroupPagesPortlet_privateLayout=false&_com_liferay_layout_admin_web_portlet_GroupPagesPortlet_displayStyle=miller-columns&_com_liferay_layout_admin_web_portlet_GroupPagesPortlet_mvcRenderCommandName=%2Flayout_admin%2Fedit_layout&_com_liferay_layout_admin_web_portlet_GroupPagesPortlet_portletResource=com_liferay_layout_admin_web_portlet_GroupPagesPortlet&p_r_p_selPlid=${plid}&_com_liferay_layout_admin_web_portlet_GroupPagesPortlet_screenNavigationEntryKey=${screenNavigationEntryKey}");
	}

	@summary = "Default summary"
	macro openURL() {
		WaitForSPARefresh();

		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		Open(value1 = "${baseURL}/web/guest/home");
	}

	@summary = "Default summary"
	macro openWithAppendToBaseURL(urlAppend = null) {
		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		Open(value1 = "${baseURL}/${urlAppend}");
	}

	@summary = "Default summary"
	macro selectSpecificWindow(windowTitle = null) {
		SelectWindow(value1 = "title=${windowTitle}");
	}

	@summary = "Default summary"
	macro viewActivatedNavTab(navTab = null) {
		AssertElementPresent(
			key_tab = ${navTab},
			locator1 = "NavTab#ACTIVE_TAB_LINK");
	}

	@summary = "Default summary"
	macro viewNavTab(navTab = null) {
		AssertVisible(
			key_tab = ${navTab},
			locator1 = "NavTab#TAB_LINK");
	}

	@summary = "Default summary"
	macro viewNoNavTab(navTab = null) {
		AssertElementNotPresent(
			key_tab = ${navTab},
			locator1 = "NavTab#TAB_LINK");
	}

}