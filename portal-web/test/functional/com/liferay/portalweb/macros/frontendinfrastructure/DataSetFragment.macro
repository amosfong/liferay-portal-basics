definition {

	@summary = "Default summary"
	macro addDataSetView(dataSetViewName = null) {
		PageEditor.gotoConfiguration(fragmentName = "Data Set");

		Click.mouseOverClick(
			ariaLabel = "Select Data Set View",
			locator1 = "Button#ANY_WITH_ARIA_LABEL");

		if (IsElementPresent(key_menuItem = "Select Data Set View...", locator1 = "MenuItem#ANY_MENU_ITEM")) {
			MenuItem.click(menuItem = "Select Data Set View...");
		}

		SelectFrame(
			key_title = "Select",
			locator1 = "IFrame#MODAL_ANY");

		Click.mouseOverClick(
			key_itemName = ${dataSetViewName},
			locator1 = "DataSet#NAME_OF_VIEWS");

		Button.clickSave();
	}

	@summary = "Default summary"
	macro setUpDataSetPage(dataSetViewName = null, pageName = null) {
		Navigator.openURL();

		ContentPages.addPage(pageName = ${pageName});

		PageEditor.addFragment(
			collectionName = "Content Display",
			fragmentName = "Data Set");

		DataSetFragment.addDataSetView(dataSetViewName = ${dataSetViewName});

		SelectFrameTop();

		Button.clickPublish();

		Navigator.openURL();

		Navigator.gotoPage(pageName = ${pageName});
	}

	@summary = "Default summary"
	macro viewColumnTitle(key_colunmTitleList = null) {
		for (var key_colunmTitle : list ${key_colunmTitleList}) {
			AssertElementPresent(
				key_field = ${key_colunmTitle},
				locator1 = "FrontendDataSet#TABLE_ITEM_COLUMN");
		}
	}

}