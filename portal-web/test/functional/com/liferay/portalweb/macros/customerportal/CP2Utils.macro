	@summary = "Default summary"
	macro finishActivation(githubUserName = null,firstName = null,lastName = null,primaryRegion = null,disasterRegion = null,projectId = null,email = null) {
		Button.click(button = "Finish Activation");

		PortletEntry.inputText(
			fieldLabel = "Project ID",
			text = ${projectId});

		Select(
			key_fieldLabel = "Primary Data Center Region",
			locator1 = "CP2Utils#LABEL_GENERIC_SELECT",
			value1 = ${primaryRegion});

		Select(
			key_fieldLabel = "Disaster Recovery Data Center Region",
			locator1 = "CP2Utils#LABEL_GENERIC_SELECT",
			value1 = ${disasterRegion});

		Type(
			locator1 = "TextInput#ANY_WITH_NAME",
			name = "dxp.admins[0].email",
			value1 = ${email});

		Type(
			locator1 = "TextInput#ANY_WITH_NAME",
			name = "dxp.admins[0].firstName",
			value1 = ${firstName});

		Type(
			locator1 = "TextInput#ANY_WITH_NAME",
			name = "dxp.admins[0].lastName",
			value1 = ${lastName});

		Type(
			locator1 = "TextInput#ANY_WITH_NAME",
			name = "dxp.admins[0].github",
			value1 = ${githubUserName});

		Button.click(button = "Next");

		Button.click(button = "Submit");

		if (isSet(doneButton)) {
			Button.click(button = "Done");
		}
	}

	@summary = "Default summary"
	macro fulfillGuestForm(companyName = null,clustered = null,explainField = null,typeOfServer = null,supportRegion = null,email = null) {
		CP2Utils.fulfillGuestFormFields(
			fieldInput = ${email},
			fieldName = "Contact Email Address",
			typeField = "true");

		CP2Utils.fulfillGuestFormFields(
			fieldInput = ${companyName},
			fieldName = "Company Name",
			typeField = "true");

		CP2Utils.fulfillGuestFormFields(
			fieldInput = ${supportRegion},
			fieldName = "Support Region");

		KeyPress(
			key_option = "Sup",
			locator1 = "CP2Utils#GUEST_FORM_OPTIONS",
			value1 = "\ENTER");

		CP2Utils.fulfillGuestFormFields(
			fieldInput = ${explainField},
			fieldName = "Please explain why you need a replacement activation key",
			typeField = "true");

		CP2Utils.fulfillGuestFormFields(
			fieldInput = ${typeOfServer},
			fieldName = "Type of Server");

		KeyPress(
			key_option = "Server",
			locator1 = "CP2Utils#GUEST_FORM_OPTIONS",
			value1 = "\ENTER");

		CP2Utils.fulfillGuestFormFields(
			fieldInput = ${clustered},
			fieldName = "Clustered?");

		KeyPress(
			key_option = "Clustered",
			locator1 = "CP2Utils#GUEST_FORM_OPTIONS",
			value1 = "\ENTER");

		Click.clickNoWaitForVisible(
			key_fieldName = "acknowledgement",
			locator1 = "Checkbox#CHECKBOX_NAME");

		if (isSet(submit)) {
			Button.click(button = "Submit");
		}
	}

	@summary = "Default summary"
	macro fulfillGuestFormFields(fieldName = null,fieldInput = null) {
		if (isSet(typeField)) {
			Type(
				key_fieldsname = ${fieldName},
				locator1 = "CP2Utils#GUEST_FORM_FIELDS",
				value1 = ${fieldInput});
		}
		else {
			Type(
				key_option = ${fieldName},
				locator1 = "CP2Utils#GUEST_FORM_OPTIONS",
				value1 = ${fieldInput});
		}
	}

