	@summary = "Default summary"
	macro tearDownForCloudEntries(status = null) {
		if (!(isSet(entry))) {
			var entry = "ERC-001";
		}

		if (!(isSet(entry1))) {
			var entry1 = "test@liferay.com";
		}

		if (!(isSet(portlet))) {
			var portlet = "DXP Cloud Environments";
		}

		if (!(isSet(portlet1))) {
			var portlet1 = "Admins DXP Cloud";
		}

		CustomerPortal.clearStatus(status = ${status});

		CP2Utils.deleteSpecificObjectEntry(
			entry = ${entry},
			portlet = ${portlet});

		CP2Utils.deleteSpecificObjectEntry(
			entry = ${entry1},
			portlet = ${portlet1});
	}

