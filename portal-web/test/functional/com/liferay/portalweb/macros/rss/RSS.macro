definition {

	@summary = "Default summary"
	macro gotoFeed(rssURL = null) {
		Click(locator1 = "Icon#BODY_VERTICAL_ELLIPSIS");

		MenuItem.click(menuItem = "RSS");

		SelectWindow.selectPopUp(value1 = "null");

		WaitForPageLoad();

		AssertLocation.assertPartialLocation(value1 = "${rssURL}/rss");
	}

	@summary = "Default summary"
	macro viewFeed(feedSubtitle = null, feedTitle = null, entryTitle = null, entryContent = null, rssURL = null) {
		RSS.gotoFeed(rssURL = ${rssURL});

		RSS.viewFeedTitle(
			feedSubtitle = ${feedSubtitle},
			feedTitle = ${feedTitle});

		RSS.viewFeedEntry(
			entryContent = ${entryContent},
			entryTitle = ${entryTitle});

		Close.closeWindow(locator1 = "null");
	}

	@summary = "Default summary"
	macro viewFeedEntry() {
		for (var entryTitle : list ${entryTitle}) {
			AssertTextEquals(
				key_entryTitle = ${entryTitle},
				locator1 = "RSS#ENTRY_TITLE",
				value1 = ${entryTitle});
		}

		for (var entryContent : list ${entryContent}) {
			AssertTextEquals(
				key_entryContent = ${entryContent},
				locator1 = "RSS#ENTRY_CONTENT",
				value1 = ${entryContent});
		}
	}

	@summary = "Default summary"
	macro viewFeedTitle(feedSubtitle = null, feedTitle = null) {
		AssertTextEquals(
			locator1 = "RSS#FEED_TITLE",
			value1 = ${feedTitle});

		AssertTextEquals(
			locator1 = "RSS#FEED_SUBTITLE",
			value1 = ${feedSubtitle});
	}

}