	@summary = "Default summary"
	macro viewInterestNotPresent(interestTopic = null) {
		AssertElementNotPresent(
			key_interestTopic = ${interestTopic},
			locator1 = "ACInterests#INTEREST_TOPIC");
	}

	@summary = "Default summary"
	macro viewInterestPage(pageViews = null, pageURL = null, pageName = null) {
		var key_pageName = ${pageName};

		AssertTextEquals(
			locator1 = "ACInterests#PAGE_NAME",
			value1 = ${pageName});

		if (isSet(pageURL)) {
			AssertTextEquals(
				locator1 = "ACInterests#PAGE_URL",
				value1 = ${pageURL});
		}

		if (isSet(pageViews)) {
			AssertTextEquals(
				locator1 = "ACInterests#PAGE_VIEWS",
				value1 = ${pageViews});
		}
	}

