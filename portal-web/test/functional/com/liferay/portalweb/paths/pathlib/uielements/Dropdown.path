<html>
<head>
<title>Dropdown</title>
</head>

<body>
<table border="1" cellpadding="1" cellspacing="1">
<thead>
<tr><td rowspan="1" colspan="3">Dropdown</td></tr>
</thead>

<tbody>
<tr>
	<td>ANY_DISABLED</td>
	<td>//*[contains(@class, 'dropdown')]//*[contains(@class, 'disabled') and contains(normalize-space(), '${key_text}')]</td>
	<td></td>
</tr>
<tr>
	<td>ANY_ENABLED</td>
	<td>//*[contains(@class, 'dropdown')]//*[not(contains(@class, 'disabled') and contains(text(),'${key_text}'))]</td>
	<td></td>
</tr>
<tr>
	<td>ANY_DROPDOWN_LABEL</td>
	<td>//*[@class='form-group' and contains(.,'${dropdownLabel}')] //*[@class="dropdown"]</td>
	<td></td>
	</tr>
<tr>
	<td>CAPTION</td>
	<td>//div[contains(@class,'caption') and normalize-space()='${key_caption}']</td>
	<td></td>
</tr>
<tr>
	<td>COMBOBOX_WITH_LABEL</td>
	<td>//label[normalize-space()='${key_fieldLabel}']//following-sibling::div//button[@role='combobox']</td>
	<td></td>
</tr>
<tr>
	<td>DROPDOWN_SUBHEADER</td>
	<td>//div[contains(@class,'dropdown-menu')]//li[contains(@class,'dropdown-subheader')][normalize-space() = '${subHeader}']</td>
	<td></td>
</tr>
<tr>
	<td>DROPDOWN_SUBHEADER_BY_INDEX</td>
	<td>xpath=(//div[contains(@class,'dropdown-menu')]//li[contains(@class,'dropdown-subheader')])[${index}][normalize-space() = '${subHeader}']</td>
	<td></td>
</tr>
<tr>
	<td>DROPDOWN_TOGGLE_BUTTON</td>
	<td>//div[contains(@class,'btn-group')]//a[contains(.,'${key_toggleButtonName}')]</td>
	<td></td>
</tr>
<tr>
	<td>DROPDOWN_TOGGLE_BUTTON_WITH_LABEL</td>
	<td>//label[normalize-space()='${key_fieldLabel}']//following-sibling::div//button[contains(@class,'dropdown-toggle')] | //label[normalize-space()='${key_fieldLabel}']//following-sibling::button[contains(@class,'select')]</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_FIELD</td>
	<td>//div[contains(@class,'dropdown-menu show')]//input[contains(@placeholder,'Search')]</td>
	<td></td>
</tr>
<tr>
	<td>EMPTY_MESSAGE_DROPDOWN</td>
	<td>//div[contains(@class,'empty-result-message')]//button[contains(@class,'dropdown-toggle')]</td>
	<td></td>
</tr>
<tr>
	<td>ENTRIES</td>
	<td>//*[contains(@class,'dropdown') and contains(.,'Entries')]</td>
	<td></td>
</tr>
<tr>
	<td>ENTRIES_NUMBER</td>
	<td>//div[@class='dropdown pagination-items-per-page']//button[contains(text(),'${key_numberEntries}')]</td>
	<td></td>
</tr>
<tr>
	<td>FRIENDLY_URL_LOCALE</td>
	<td>//button[contains(@id,'friendlyURLMenu')]//span[contains(@class,'btn-section')]</td>
	<td></td>
</tr>
<tr>
	<td>INFO</td>
	<td>//div[contains(@class,'dropdown-menu')][contains(@class,'show')]//div[contains(@class,'alert-info')][normalize-space(text())='${infoMessage}']</td>
	<td></td>
</tr>
<tr>
	<td>TRIGGER</td>
	<td>//div[contains(@class,'ddm-row')]//button[@role='combobox']</td>
	<td></td>
</tr>
<tr>
	<td>TRIGGER_2</td>
	<td>xpath=(//div[contains(@class,'ddm-row')])[2]//button[@role='combobox']</td>
	<td></td>
</tr>
<tr>
	<td></td>
	<td></td>
	<td></td>
</tr>

<!--FILTER-->

<tr>
	<td>FILTER</td>
	<td>//span[contains(text(), 'Filter')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>