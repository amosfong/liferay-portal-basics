@default = "verifyElementNotPresent"
@summary = "Verify that '${locator1}' is not available"
definition {

	function verifyElementNotPresent(locator1) {
		WaitForSPARefresh();

		selenium.waitForElementNotPresent(${locator1}, "false");

		selenium.verifyElementNotPresent(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

}