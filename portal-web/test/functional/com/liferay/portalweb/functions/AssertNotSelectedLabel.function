@default = "assertNotSelectedLabel"
definition {

	function assertNotSelectedLabel(locator1, value2, value1) {
		WaitForSPARefresh();

		selenium.waitForVisible(${locator1}, "false");

		selenium.assertNotSelectedLabel(${value1}, ${value2});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

}