@default = "scrollWebElementIntoView"
definition {

	function scrollWebElementIntoView(locator1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.scrollWebElementIntoView(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

}