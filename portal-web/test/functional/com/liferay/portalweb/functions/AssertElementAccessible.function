@default = "assertElementAccessible"
definition {

	function assertElementAccessible(locator1, value1) {
		WaitForSPARefresh();

		if (isSet(value1)) {
			var ignorableImpacts = ListUtil.newListFromString(${value1});
		}
		else {
			var ignorableImpacts = ListUtil.newList();
		}

		selenium.assertElementAccessible(${locator1}, ${ignorableImpacts});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

}