@default = "assertNotChecked"
definition {

	function assertNotChecked(locator1) {
		WaitForSPARefresh();

		selenium.waitForVisible(${locator1}, "false");

		selenium.assertNotChecked(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function assertNotCheckedHiddenToggleSwitch(locator1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.assertNotChecked("xpath=(${locator1})/../../input");

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function assertNotCheckedNotVisible(locator1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.assertNotChecked(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

}