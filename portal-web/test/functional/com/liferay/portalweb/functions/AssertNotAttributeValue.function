@default = "assertNotAttributeValue"
definition {

	function assertNotAttributeValue(locator1, value1, attribute1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.waitForVisible(${locator1}, "false");

		selenium.assertNotAttributeValue(${attribute1}, ${locator1}, ${value1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

}