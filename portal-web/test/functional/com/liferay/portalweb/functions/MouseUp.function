@default = "mouseUp"
definition {

	function mouseUp(locator1) {
		WaitForSPARefresh();

		selenium.mouseUp(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function mouseUpAt(locator1, value1) {
		WaitForSPARefresh();

		selenium.waitForVisible(${locator1}, "false");

		selenium.mouseUpAt(${locator1}, ${value1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

}