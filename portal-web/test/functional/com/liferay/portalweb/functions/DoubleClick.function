@default = "doubleClick"
definition {

	function doubleClick(locator1) {
		WaitForSPARefresh();

		selenium.waitForVisible(${locator1}, "false");

		selenium.mouseOver(${locator1});

		selenium.doubleClick(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function doubleClickAt(locator1, value1) {
		WaitForSPARefresh();

		selenium.waitForVisible(${locator1}, "false");

		selenium.mouseOver(${locator1});

		selenium.doubleClickAt(${locator1}, ${value1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function doubleClickNotVisible(locator1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.mouseOver(${locator1});

		selenium.doubleClick(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function javaScriptDoubleClick(locator1) {
		WaitForSPARefresh();

		selenium.waitForVisible(${locator1}, "false");

		selenium.mouseOver(${locator1});

		selenium.javaScriptDoubleClick(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

}