@default = "clickAt"
definition {

	function clickAt(locator1, value1) {
		WaitForSPARefresh();

		selenium.waitForVisible(${locator1}, "false");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});
		}

		AssertNotChecked.assertNotChecked();

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function uncheckHiddenToggleSwitch(locator1, value1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked("xpath=(${locator1})/../../input")) {
			selenium.clickAt(${locator1}, ${value1});
		}

		AssertNotChecked.assertNotCheckedHiddenToggleSwitch();

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function uncheckNotVisible(locator1, value1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});

			selenium.pause(1000);
		}

		selenium.assertNotChecked(${locator1});

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function uncheckToggleSwitch(locator1, value1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});
		}

		AssertNotChecked.assertNotCheckedNotVisible();

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function uncheckToggleSwitchAndPause(locator1, value1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});
		}

		selenium.pause(1000);

		AssertNotChecked.assertNotCheckedNotVisible();

		selenium.assertJavaScriptErrors("true");

		selenium.assertLiferayErrors();
	}

	function uncheckToggleSwitchNoErrorNoAssert(locator1, value1) {
		WaitForSPARefresh();

		selenium.waitForElementPresent(${locator1}, "false");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});
		}
	}

}