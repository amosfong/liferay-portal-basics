{
	"concurrencyPolicy": "Forbid",
	"cpu": 4,
	"env": {
		"LIFERAY_JAR_RUNNER_JAVA_OPTS": "-Xmx4096m",
		"LIFERAY_LEARN_ETC_CRON_GITHUB_BRANCH": "master",
		"LIFERAY_LEARN_ETC_CRON_GITHUB_DEPLOY_KEY": "@liferay-learn-github-deploy-key",
		"LIFERAY_LEARN_ETC_CRON_GITHUB_USER": "liferay",
		"LIFERAY_LEARN_ETC_CRON_GIT_REPOSITORY_DIR": "/home/liferay/liferay-learn",
		"LIFERAY_LEARN_ETC_CRON_LIFERAY_DATA_DEFINITION_KEY": "LEARN-ARTICLE",
		"LIFERAY_LEARN_ETC_CRON_LIFERAY_OAUTH_CLIENT_ID": "@liferay-oauth-client-id",
		"LIFERAY_LEARN_ETC_CRON_LIFERAY_OAUTH_CLIENT_SECRET": "@liferay-oauth-client-secret",
		"LIFERAY_LEARN_ETC_CRON_LIFERAY_SITE_FRIENDLY_URL_PATH": "guest",
		"LIFERAY_LEARN_ETC_CRON_LIFERAY_URL": "https://learn-dev.liferay.com",
		"LIFERAY_LEARN_ETC_CRON_SLACK_CHANNEL": "#is-sites-learn-devops-dev",
		"LIFERAY_LEARN_ETC_CRON_SLACK_ENDPOINT": "@slack-endpoint",
		"LIFERAY_LEARN_ETC_SKIP_DIFF_CHECK": false,
		"LIFERAY_LEARN_RESOURCE_DOMAIN": "https://resources.learn-dev.liferay.com",
		"LIFERAY_ROUTES_CLIENT_EXTENSION": "/etc/liferay/lxc/ext-init-metadata",
		"LIFERAY_ROUTES_DXP": "/etc/liferay/lxc/dxp-metadata"
	},
	"environments": {
		"extprd": {
			"env": {
				"LIFERAY_LEARN_ETC_CRON_LIFERAY_URL": "https://learn.liferay.com",
				"LIFERAY_LEARN_ETC_CRON_SLACK_CHANNEL": "#is-sites-learn-devops-prd",
				"LIFERAY_LEARN_RESOURCE_DOMAIN": "https://resources.learn.liferay.com"
			},
			"schedule": "0 15 * * *"
		},
		"extuat": {
			"env": {
				"LIFERAY_LEARN_ETC_CRON_LIFERAY_URL": "https://learn-uat.liferay.com",
				"LIFERAY_LEARN_ETC_CRON_SLACK_CHANNEL": "#is-sites-learn-devops-uat",
				"LIFERAY_LEARN_RESOURCE_DOMAIN": "https://resources.learn-uat.liferay.com"
			},
			"schedule": "0 15 * * *"
		},
		"infra": {
			"deploy": false
		}
	},
	"id": "__PROJECT_ID__",
	"kind": "CronJob",
	"memory": 3000,
	"scale": 1,
	"schedule": "0 10 * * 1",
	"volumes": {
		"homeLiferay": "/home/liferay",
		"publicHtml": "/public_html"
	}
}