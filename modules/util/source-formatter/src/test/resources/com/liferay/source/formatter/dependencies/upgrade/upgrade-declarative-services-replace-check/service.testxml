<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder auto-import-default-references="false" auto-namespace-tables="false" dependency-injector="spring" mvcc-enabled="true" package-path="com.test.declarative.services">
	<namespace>DeclarativeServicesEntry</namespace>
	<entity local-service="true" name="DeclarativeServicesEntry" remote-service="false" trash-enabled="false" uuid="true">

		<!-- PK fields -->

		<column name="declarativeServicesEntryId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="createDate" type="Date" />

		<!-- Finder methods -->

		<finder name="CompanyId" return-type="Collection">
			<finder-column name="companyId" />
		</finder>
	</entity>
</service-builder>