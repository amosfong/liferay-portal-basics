@default = "waitForPageLoad"
definition {

	function waitForPageLoad(value2, value1, value3) {
		if (!(isSet(locator1))) {
			var locator1 = 0;
		}

		var javascript = '''
			window.onload = function() {
				setTimeout(() => {
					console.log("Page is fully loaded");
				}, "${locator1}");
			};
		''';

		selenium.executeJavaScript(${value1}, ${value2}, ${value3});
	}

}