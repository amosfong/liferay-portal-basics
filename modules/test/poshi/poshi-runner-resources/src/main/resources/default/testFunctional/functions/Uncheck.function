@default = "clickAt"
definition {

	function clickAt(locator1, value1) {
		selenium.waitForVisible(${locator1}, "true");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});
		}

		AssertNotChecked.assertNotChecked();
	}

	function uncheckNotVisible(locator1, value1) {
		selenium.waitForElementPresent(${locator1}, "true");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});

			selenium.pause(1000);
		}

		selenium.assertNotChecked(${locator1});
	}

	function uncheckToggleSwitch(locator1, value1) {
		selenium.waitForElementPresent(${locator1}, "true");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});
		}

		AssertNotChecked.assertNotCheckedNotVisible();
	}

	function uncheckToggleSwitchAndPause(locator1, value1) {
		selenium.waitForElementPresent(${locator1}, "true");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});
		}

		selenium.pause(1000);

		AssertNotChecked.assertNotCheckedNotVisible();
	}

	function uncheckToggleSwitchNoErrorNoAssert(locator1, value1) {
		selenium.waitForElementPresent(${locator1}, "true");

		selenium.mouseOver(${locator1});

		if (selenium.isChecked(${locator1})) {
			selenium.clickAt(${locator1}, ${value1});
		}
	}

}