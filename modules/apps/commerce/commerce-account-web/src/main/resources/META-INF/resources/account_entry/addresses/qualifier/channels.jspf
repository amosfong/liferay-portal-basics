--%>

<div id="item-finder-root-channel"></div>

<liferay-frontend:component
	context='<%=
		HashMapBuilder.<String, Object>put(
			"addressExternalReferenceCode", address.getExternalReferenceCode()
		).put(
			"addressId", addressId
		).put(
			"dataSetId", CommerceAccountFDSNames.ACCOUNT_ADDRESS_QUALIFIER_CHANNELS
		).put(
			"rootPortletId", portletDisplay.getRootPortletId()
		).put(
			"spritemap", themeDisplay.getPathThemeSpritemap()
		).build()
	%>'
	module="{channels} from commerce-account-web"
/>

<commerce-ui:panel
	bodyClasses="p-0"
	title='<%= LanguageUtil.get(request, "channels") %>'
>
	<frontend-data-set:headless-display
		apiURL="<%= accountAddressQualifiersDisplayContext.getAccountAddressChannelsAPIURL() %>"
		fdsActionDropdownItems="<%= accountAddressQualifiersDisplayContext.getAccountAddressChannelFDSActionDropdownItems() %>"
		formName="fm"
		id="<%= CommerceAccountFDSNames.ACCOUNT_ADDRESS_QUALIFIER_CHANNELS %>"
		itemsPerPage="<%= 10 %>"
	/>
</commerce-ui:panel>