	@summary = "Default summary"
	macro _addDiscountSku(productSKU = null, discountTitle = null, unitOfMeasureKey = null) {
		Variables.assertDefined(parameterList = "${discountTitle},${productSKU}");

		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		if (!(isSet(discountId))) {
			var discountId = CommerceJSONDiscountsAPI._getDiscountId(discountTitle = ${discountTitle});
		}

		if (isSet(unitOfMeasureKey)) {
			var body = '''
				-d {
					"skuId": ${skuId},
					"unitOfMeasureKey": "${unitOfMeasureKey}"
				}
			''';
		}
		else {
			var body = '''
				-d '{
					"skuId": ${skuId}
				}'
			''';
		}

		var skuId = CommerceJSONProductsAPI._getSKUId(productSKU = ${productSKU});
		var userLoginInfo = JSONUtil2.formatJSONUser();

		var curl = '''
			${baseURL}/o/headless-commerce-admin-pricing/v2.0/discounts/${discountId}/discount-skus \
				-u ${userLoginInfo} \
				-H 'accept: application/json' \
				-H 'Content-Type: application/json' \
		''';

		var curl = StringUtil.add(${curl}, ${body}, " ");

		com.liferay.poshi.runner.util.JSONCurlUtil.post(${curl});

		return ${discountId};
	}

	@summary = "Default summary"
	macro _deleteAllCommerceDiscounts() {
		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		var discountsCount = CommerceJSONDiscountsAPI._getCommerceDiscountsCount();

		if (${discountsCount} != 0) {
			var discountIds = CommerceJSONDiscountsAPI._getCommerceDiscountIds(discountCount = ${discountsCount});

			for (var discountId : list ${discountIds}) {
				echo("Deleting Commerce discount with id: ${discountId}");

				var userLoginInfo = JSONUtil2.formatJSONUser();

				var curl = '''
					${baseURL}/o/headless-commerce-admin-pricing/v2.0/discounts/${discountId} \
						-u ${userLoginInfo} \
						-H 'accept: application/json' \
				''';

				JSONCurlUtil.delete(${curl});
			}
		}
		else {
			echo("No Commerce Discounts to be deleted");
		}
	}

	@summary = "Default summary"
	macro _deleteDiscountSkuUom(productSKU = null, discountTitle = null, uomKey = null) {
		Variables.assertDefined(parameterList = ${uomKey});

		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = JSONCompany.getPortalURL();
		}

		var responseBody = CommerceJSONDiscountsAPI._getCommerceDiscountSkuById(discountTitle = ${discountTitle});
		var skuId = CommerceJSONProductsAPI._getSKUId(productSKU = ${productSKU});

		var discountSkuId = JSONUtil.getWithJSONPath(${responseBody}, "$..items[?(@['unitOfMeasureKey'] == '${uomKey}' && @['skuId'] == ${skuId})].['discountSkuId']");
		var userLoginInfo = JSONUtil2.formatJSONUser();

		var curl = '''
			${baseURL}/o/headless-commerce-admin-pricing/v2.0/discount-skus/${discountSkuId} \
				-u ${userLoginInfo} \
				-H 'accept: application/json' \
		''';

		JSONCurlUtil.delete(${curl});
	}

