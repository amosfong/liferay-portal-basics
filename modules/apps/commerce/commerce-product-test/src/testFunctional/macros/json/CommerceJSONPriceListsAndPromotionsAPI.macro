	}

	@summary = "Default summary"
	macro _getPriceEntryId(productSKU = null, priceListName = null) {
		Variables.assertDefined(parameterList = "${priceListName},${productSKU}");

		var baseURL = ${baseURL};

		if (!(isSet(baseURL))) {
			var baseURL = PropsUtil.get("portal.url");
		}

		if (isSet(priceEntriesCount)) {
			var priceEntriesCount = ${priceEntriesCount};
		}
		else {
			var priceEntriesCount = 100;
		}

		if (!(isSet(unitOfMeasureKey))) {
			var unitOfMeasureKey = "";
		}

		var priceListId = CommerceJSONPriceListsAndPromotionsAPI._getCommercePriceListsAndPromotionsIdByName(priceListAndPromotionName = ${priceListName});
		var skuId = CommerceJSONProductsAPI._getSKUId(productSKU = ${productSKU});
		var userLoginInfo = JSONUtil2.formatJSONUser();

		var curl = '''
			${baseURL}/o/headless-commerce-admin-pricing/v2.0/price-lists/${priceListId}/price-entries?pageSize=${priceEntriesCount} \
				-u ${userLoginInfo} \
				-H 'accept: application/json' \
		''';

		var priceEntryId = JSONCurlUtil.get(${curl}, "$.items[?(@['unitOfMeasureKey'] == '${unitOfMeasureKey}' && @['skuId'] == ${skuId})].['priceEntryId']");

		echo("The Commerce price entry id is: ${priceEntryId}");

		return ${priceEntryId};
	}

