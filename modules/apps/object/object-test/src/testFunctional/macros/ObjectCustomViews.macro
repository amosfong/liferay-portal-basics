definition {

	@summary = "Default summary"
	macro addColumnsViaUI(addColumns = null) {
		if (IsElementPresent(key_text = "No columns added yet", locator1 = "ObjectCustomViews#ADD_COLUMN")) {
			Click(locator1 = "ObjectCustomViews#ADD_COLUMN");
		}
		else {
			Click(locator1 = "Button#PLUS");
		}

		SelectFrame(locator1 = "ObjectCustomViews#VIEW_MODAL_CONTENT");

		for (var columnsOptions : list ${addColumns}) {
			Click.clickAtNotVisible(
				key_columnOption = ${columnsOptions},
				locator1 = "ObjectCustomViews#CHECKBOX_COLUMN_OPTION");
		}

		PortletEntry.save();

		SelectFrame(locator1 = "IFrame#IFRAME");
	}

	@summary = "Default summary"
	macro addDefaultSort(columnOption = null, sortOption = null) {
		if (IsElementPresent(key_text = "No default sort was created yet", locator1 = "ObjectCustomViews#VIEW_BUILDER_EMPTY_STATE_TEXT")) {
			ObjectCustomViews.addNewDefaultSort();
		}
		else {
			Click(locator1 = "ObjectCustomViews#DEFAULT_SORT_ADD_BUTTON");
		}

		ObjectAdmin.selectDropdownItem(
			labelName = "Columns",
			optionName = ${columnOption});

		ObjectAdmin.selectDropdownItem(
			labelName = "Sorting",
			optionName = ${sortOption});

		Click(
			key_tab = "Save",
			locator1 = "Button#TAB_SUBMIT_BTN");
	}

	@summary = "Default summary"
	macro addNewAggregationFilterViaUI(dateValueList = null, filterValue = null, filterBy = null, filterType = null) {
		Click(locator1 = "ObjectCustomViews#ADD_NEW_FILTER");

		Click(locator1 = "ObjectCustomViews#VIEW_MODAL_CONTENT");

		Click(
			key_optionName = ${filterBy},
			locator1 = "ObjectAdmin#CLAY_GENERIC_DROPDOWN_ITEM");

		ObjectAdmin.selectDropdownItem(
			labelName = "Filter Type",
			optionName = ${filterType});

		if (${filterType} == "Range") {
			var dateIndex = 1;

			for (var dateValue : list ${dateValueList}) {
				Type(
					key_dateIndex = ${dateIndex},
					locator1 = "ObjectCustomViews#FIELD_VIEW_FILTER_DATE_INPUT",
					value1 = ${dateValue});

				var dateIndex = ${dateIndex} + 1;
			}
		}
		else {
			Type(
				key_text = "Value",
				locator1 = "TextInput#ANY",
				value1 = ${filterValue});
		}

		ObjectCustomViews.clickSaveOnModal();
	}

	@summary = "Default summary"
	macro addNewDefaultSort() {
		Click(locator1 = "ObjectCustomViews#ADD_NEW_DEFAULT_SORT");
	}

	@summary = "Default summary"
	macro addNewFilterViaUI(filterValue = null, filterBy = null, filterType = null) {
		if (IsElementPresent(key_text = "No filter was created yet", locator1 = "ObjectCustomViews#ADD_NEW_FILTER")) {
			Click(locator1 = "ObjectCustomViews#ADD_NEW_FILTER");
		}
		else {
			Click(locator1 = "ObjectCustomViews#FILTER_ADD_BUTTON");
		}

		Click(locator1 = "ObjectCustomViews#VIEW_MODAL_CONTENT");

		MenuItem.clickNoError(menuItem = ${filterBy});

		if (isSet(filterType)) {
			ObjectAdmin.selectDropdownItem(
				labelName = "Filter Type",
				optionName = ${filterType});
		}

		if (isSet(filterValue)) {
			Click(
				key_labelName = "Value",
				locator1 = "ObjectAdmin#CLAY_MULTI_SELECT_INPUT");

			for (var filterValueList : list ${filterValue}) {
				Click.clickAtNotVisible(
					key_columnOption = ${filterValueList},
					locator1 = "ObjectCustomViews#CHECKBOX_COLUMN_OPTION");
			}
		}

		ObjectCustomViews.clickSaveOnModal();
	}

	@summary = "Default summary"
	macro addObjectViewViaUI(viewName = null) {
		LexiconEntry.gotoAdd();

		Type(
			locator1 = "ObjectCustomViews#ADD_VIEW_NAME",
			value1 = ${viewName});

		PortletEntry.save();
	}

	@summary = "Default summary"
	macro assertColumnPresentOnDefaultSort(sortingLabel = null, columnName = null) {
		AssertElementPresent(
			key_columnName = ${columnName},
			locator1 = "ObjectCustomViews#DEFAULT_SORT_SEARCH_RESULT");

		AssertTextEquals(
			key_sortingLabel = ${sortingLabel},
			locator1 = "ObjectCustomViews#DEFAULT_SORT_SORTING_LABEL",
			value1 = ${sortingLabel});
	}

	@summary = "Default summary"
	macro assertColumnVisibleOnAddColumnModal(columnOption = null) {
		SelectFrame(locator1 = "ObjectCustomViews#VIEW_MODAL_CONTENT");

		AssertVisible(
			key_columnName = ${columnOption},
			locator1 = "ObjectCustomViews#ADD_COLUMNS_COLUMN_LABEL");
	}

	@summary = "Default summary"
	macro assertFilterPresent(filterBy = null) {
		Click(locator1 = "ObjectCustomViews#ADD_NEW_FILTER");

		Click(locator1 = "ObjectCustomViews#VIEW_MODAL_CONTENT");

		AssertElementPresent(
			key_menuItem = ${filterBy},
			locator1 = "MenuItem#ANY_MENU_ITEM");
	}

	@summary = "Default summary"
	macro assertObjectView(viewLabel = null) {
		var key_viewLabel = ${viewLabel};

		AssertElementPresent(locator1 = "ObjectCustomViews#OBJECT_VIEW_LABEL");
	}

	@summary = "Default summary"
	macro assertPresentEntriesOnFilters(filterValue = null, filterBy = null) {
		AssertElementPresent(
			key_columnName = ${filterBy},
			locator1 = "ObjectCustomViews#VIEW_BUILDER_SEARCH_RESULT");

		if (isSet(filterValue)) {
			for (var filterValueList : list ${filterValue}) {
				AssertElementPresent(
					key_columnLabel = ${filterValueList},
					locator1 = "ObjectCustomViews#BUILDER_SEARCH_RESULT_COLUMN_LABEL");
			}
		}
	}

	@summary = "Default summary"
	macro clickOnFilterOnEntries() {
		Click(
			key_buttonName = "Filter",
			locator1 = "ObjectCustomViews#DROPDOWN_BUTTON_ON_OBJECT_VIEW");

		SelectFrame(locator1 = "ObjectCustomViews#VIEW_DROPDOWN_MENU_ON_MODAL_DEFAULT_SORT");
	}

	@summary = "Default summary"
	macro clickSaveOnModal() {
		Click(
			key_name = "Save",
			locator1 = "Button#GENERAL_BUTTON_MODAL_FOOTER");
	}

	@summary = "Default summary"
	macro defineDateOnFilter(filterName = null, displayDateMonth = null, displayDateYear = null, displayDateDay = null) {
		var displayDate = "${displayDateMonth}/${displayDateDay}/${displayDateYear}";

		Type(
			locator1 = "ObjectCustomViews#SEARCH_DROPDOWN_MENU_FILTER",
			value1 = ${filterName});

		Click(
			key_fieldName = ${filterName},
			locator1 = "FormViewBuilder#OBJECT_FIELD_BUTTON_DROPDOWN");

		Type(
			key_fieldName = "yyyy-mm-dd",
			locator1 = "ObjectCustomViews#FROM_DATE_INPUT",
			value1 = ${displayDate});

		Click(
			key_text = "Add Filter",
			locator1 = "Button#ANY");
	}

	@summary = "Default summary"
