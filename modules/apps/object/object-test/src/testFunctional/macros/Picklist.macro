	@summary = "Default summary"
	macro assertDownloadedPicklist(downloadedFile = null) {
		var downloadedFileName = TestCase.getDownloadedTempFileName(fileNamePattern = ${downloadedFile});

		echo("## downloadedFileName: ListType_*");

