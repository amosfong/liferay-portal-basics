	@summary = "Default summary"
	macro addPicklistViaUI(picklistName = null) {
		WaitForElementPresent(locator1 = "Picklist#ADD_PICKLIST_BUTTON");

		Click.javaScriptClick(locator1 = "Picklist#ADD_PICKLIST_BUTTON");

