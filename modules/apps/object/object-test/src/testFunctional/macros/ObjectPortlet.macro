	@summary = "Default summary"
	macro addCustomFieldEntryViaUI(invalidEntry = null, entry = null, customField = null) {
		LexiconEntry.gotoAdd();

		Type.type(
			key_customField = ${customField},
			locator1 = "ObjectPortlet#ENTRY_FIELD_GENERIC",
			value1 = ${entry});

		if (${invalidEntry} == "true") {
			Button.clickSave();
		}
		else {
			PortletEntry.save();
		}
	}

