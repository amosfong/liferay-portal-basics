definition {

	@summary = "Default summary"
	macro exportObject(objectName = null, objectLabel = null) {
		ObjectAdmin.selectSpecificKebabMenuOption(
			kebabOption = "Export Object Definition",
			objectLabel = ${objectLabel});

		DownloadTempFile();

		var tempFileName = TestCase.getDownloadedTempFileName(fileNamePattern = "Object_Definition_${objectName}*.json");
		var tempFileDirectory = selenium.getOutputDirName();

		var exportedConfigFile = FileUtil.exists("${tempFileDirectory}/${tempFileName}");

		if ((${exportedConfigFile} == "true") && (${tempFileName} != "")) {
			echo("PASSED! The file ${tempFileName} exists.");
		}
		else {
			fail("FAIL! The file ${tempFileName} doesn't exists.");
		}
	}

	@summary = "Default summary"
	macro importObject(objectFileName = null, objectName = null, tempFile = null, modelBuilder = null) {
		ObjectAdmin.openObjectAdmin();

		Click(locator1 = "ObjectModelBuilder#OBJECT_DEFINITONS_KEBAB");

		MenuItem.click(menuItem = "Import Object Definition");

		if (${tempFile} == "true") {
			var tempFileName = TestCase.getDownloadedTempFileName(fileNamePattern = "Object_Definition_${objectName}*.json");
			var tempFileDirectory = selenium.getOutputDirName();

			var filePath = "${tempFileDirectory}/${tempFileName}";
		}
		else {
			var filePath = TestCase.getDependenciesDirPath(fileName = "${objectFileName}.json");
		}

		UploadFile(
			locator1 = "TextInput#FILE",
			value1 = ${filePath});

		Type(
			key_fieldLabel = "Name",
			locator1 = "TextInput#GENERIC_TEXT_INPUT",
			value1 = ${objectName});

		Button.clickImport();
	}

}