	@summary = "Default summary"
	macro gotoLookAndFeelConfiguration() {
		Portlet.gotoPortletOptions(
			portletName = "Calendar",
			portletOption = "Look and Feel Configuration");

		SelectFrame(locator1 = "IFrame#MODAL_BODY");
	}

