definition {

	@summary = "Default summary"
	macro assertModalOption(description = null, option = null) {
		if (isSet(option)) {
			AssertElementPresent(
				key_text = ${option},
				locator1 = "RaylifeApplicationModal#MODAL_INSURANCE");
		}

		if (isSet(description)) {
			AssertElementPresent(
				key_text = ${description},
				locator1 = "RaylifeApplicationModal#MODAL_DESCRIPTION");
		}
	}

	@summary = "Default summary"
	macro birthDate(nextDayDate = null) {
		Type(
			key_placeHolder = "__/__/____",
			locator1 = "TextInput#ANY_PLACEHOLDER",
			value1 = ${nextDayDate});
	}

	@summary = "Default summary"
	macro openNewApplicationFlow(insurance = null, button = null, product = null) {
		Button.click(button = "Application");

		RaylifeApplicationModal.selectModalOption(key_text = ${insurance});

		Button.click(button = "Next");

		if (isSet(button)) {
			Button.click(button = ${button});
		}

		if (isSet(product)) {
			RaylifeApplicationModal.selectModalOption(key_text = ${product});

			Button.click(button = "Next");
		}
	}

	@summary = "Default summary"
	macro selectModalOption(option = null) {
		Click(
			key_text = ${option},
			locator1 = "RaylifeApplicationModal#MODAL_OPTION");
	}

	@summary = "Default summary"
	macro selectOptionInDropdownByName(text = null) {
		Click(locator1 = "RaylifeApplicationModal#OPEN_DROPDOWN");

		Click(
			key_text = ${text},
			locator1 = "RaylifeApplicationModal#SELECT_DROPDOWN_OPTION");
	}

	@summary = "Default summary"
	macro selectOptionInModalByID(value = null) {
		Select(
			locator1 = "RaylifeApplicationModal#SELECT_OPTION",
			value1 = ${value});
	}

	@summary = "Default summary"
	macro typeAllNewApplicationContactInfoFields(firstName = null, lastName = null, zipCode = null, emailAddress = null, phoneNumber = null, apt = null, streetAddress = null, city = null, state = null) {
		var nextDayDate = DateUtil.getFormattedDate("+1", "MM/dd/yyyy");

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "firstName",
			value = ${firstName});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "lastName",
			value = ${lastName});

		RaylifeApplicationModal.birthDate(nextDayDate = ${nextDayDate});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "phone",
			value = ${phoneNumber});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "email",
			value = ${emailAddress});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "streetAddress",
			value = ${streetAddress});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "apt",
			value = ${apt});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "city",
			value = ${city});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "mySelectId",
			value = ${state});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "zipCode",
			value = ${zipCode});

		DoubleClick(
			key_option = "Own",
			locator1 = "RaylifeApplicationModal#CHOOSE_OPTION");

		Button.click(button = "Next");
	}

	@summary = "Default summary"
	macro typeAllNewApplicationCoverageInfoFields(uninsuredOrUnderinsuredMPD = null, propertyDamage = null, collisionAmount = null, uninsuredOrUnderinsuredMBI = null, medicalAmount = null, bodilyInjury = null, comprehensiveAmount = null) {
		RaylifeApplicationModal.selectOptionInModalByID(
			label = "mySelectId",
			value = ${bodilyInjury});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "propertyDamage",
			value = ${propertyDamage});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "uninsuredOrUnderinsuredMBI",
			value = ${uninsuredOrUnderinsuredMBI});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "uninsuredOrUnderinsuredMPD",
			value = ${uninsuredOrUnderinsuredMPD});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "Medical",
			value = ${medicalAmount});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "comprehensive",
			value = ${comprehensiveAmount});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "collision",
			value = ${collisionAmount});

		Button.click(button = "Review quote");
	}

	@summary = "Default summary"
	macro typeAllNewApplicationDriverInfoFields(militaryAffiliation = null, firstName = null, lastName = null, occupation = null, gender = null, highestEducation = null, ageFirstLicenced = null, maritalStatus = null, relationToContact = null) {
		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "firstName",
			value = ${firstName});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "lastName",
			value = ${lastName});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "relationToContact",
			value = ${relationToContact});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "gender",
			value = ${gender});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "maritalStatus",
			value = ${maritalStatus});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "ageFirstLicenced",
			value = ${ageFirstLicenced});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "occupation",
			value = ${occupation});

		RaylifeApplicationModal.selectOptionInModalByID(
			label = "highestEducation",
			value = ${highestEducation});

		RaylifeApplicationModal.selectOptionInDropdownByName(
			field = "millitary-affiliation",
			text = ${militaryAffiliation});

		if (isSet(addAccidentCitation)) {
			DoubleClick(
				key_option = "Yes",
				locator1 = "RaylifeApplicationModal#CHOOSE_OPTION");
		}

		if (isSet(addDriver)) {
			Click(
				key_index = 1,
				locator1 = "RaylifeApplicationModal#ANY_DRIVER_BUTTON");
		}
		else {
			Click(
				key_option = "No",
				locator1 = "RaylifeApplicationModal#CHOOSE_OPTION");

			Button.click(button = "Next");
		}
	}

	@summary = "Default summary"
	macro typeAllNewApplicationVehicleInfoFields(annualMileage = null, ownership = null, year = null, primaryUse = null, model = null, make = null) {
		RaylifeApplicationModal.selectOptionInDropdownByName(
			field = "years",
			text = ${year});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "make",
			value = ${make});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "model",
			value = ${model});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "primary-usage",
			value = ${primaryUse});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "annual-mileage",
			value = ${annualMileage});

		RaylifeApplicationModal.typeNewApplicationInfoFields(
			label = "ownership",
			value = ${ownership});

		Button.click(button = "Next");
	}

	@summary = "Default summary"
	macro typeNewApplicationInfoFields(value = null) {
		Type(
			locator1 = "Raylife#FORMS_FIELDS",
			value1 = ${value});
	}

}