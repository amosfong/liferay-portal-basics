definition {

	@summary = "Default summary"
	macro createOrganization(country = null,zipCode = null,bankingInfo = null,organizationName = null,city = null,contactEmail = null,contactName = null,organizationSite = null,smallDescription = null,key_status = null,organizationId = null,street = null,taxId = null,state = null,accountOwnerName = null,contactPhone = null,status = null) {
		if (!(isSet(messageEVPManager))) {
			var messageEVPManager = "";
		}

		JSONEVPAPI.addOrganization(
			accountOwnerName = ${accountOwnerName},
			bankingInfo = ${bankingInfo},
			city = ${city},
			contactEmail = ${contactEmail},
			contactName = ${contactName},
			contactPhone = ${contactPhone},
			country = ${country},
			emailAddress = ${emailAddress},
			key_status = ${key_status},
			messageEVPManager = ${messageEVPManager},
			organizationId = ${organizationId},
			organizationName = ${organizationName},
			organizationSite = ${organizationSite},
			smallDescription = ${smallDescription},
			state = ${state},
			status = ${status},
			street = ${street},
			taxId = ${taxId},
			zipCode = ${zipCode});
	}

	@summary = "Default summary"
	macro createRequest(requestType = null,requestPurposesKey = null,requestBehalfKey = null,fullName = null,requestBehalf = null,managerEmailAddress = null,requestStatusKey = null,totalHoursRequested = null,grantAmount = null,requestDescription = null,organizationId = null,requestPurposes = null,emailAddress = null,phoneNumber = null,grantRequestTypeKey = null,grantRequestType = null,requestStatus = null) {
		if (!(isSet(messageEVPManager))) {
			var messageEVPManager = "";
		}

		var startDate = DateUtil.getFormattedDate(0, "yyyy-MM-dd");
		var endDate = DateUtil.getFormattedDate("+2", "yyyy-MM-dd");

		if (${requestType} == "Grant") {
			JSONEVPAPI.addRequest(
				emailAddress = ${emailAddress},
				fullName = ${fullName},
				grantAmount = ${grantAmount},
				grantRequestType = ${grantRequestType},
				grantRequestTypeKey = ${grantRequestTypeKey},
				messageEVPManager = ${messageEVPManager},
				organizationId = ${organizationId},
				phoneNumber = ${phoneNumber},
				requestDescription = ${requestDescription},
				requestStatus = ${requestStatus},
				requestStatusKey = ${requestStatusKey},
				requestType = ${requestType},
				requestTypeKey = "grant");
		}

		if (${requestType} == "Service") {
			JSONEVPAPI.addRequest(
				emailAddress = ${emailAddress},
				endDate = ${endDate},
				fullName = ${fullName},
				managerEmailAddress = ${managerEmailAddress},
				messageEVPManager = ${messageEVPManager},
				organizationId = ${organizationId},
				phoneNumber = ${phoneNumber},
				requestDescription = ${requestDescription},
				requestStatus = ${requestStatus},
				requestStatusKey = ${requestStatusKey},
				requestType = ${requestType},
				requestTypeKey = "service",
				startDate = ${startDate},
				totalHoursRequested = ${totalHoursRequested});
		}
	}

}