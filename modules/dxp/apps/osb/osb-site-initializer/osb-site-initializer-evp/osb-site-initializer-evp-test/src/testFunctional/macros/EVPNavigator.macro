definition {

	@summary = "Default summary"
	macro assertActiveBreadcrumb(activeItem = null) {
		AssertElementPresent(
			locator1 = "Breadcrumb#BREADCRUMB_ENTRY_ACTIVE_ENTRY",
			value1 = ${activeItem});
	}

	@summary = "Default summary"
	macro goToNavItem(navItem = null) {
		Click(
			key_tab = ${navItem},
			locator1 = "NavTab#TAB_LINK");

		WaitForPageLoad();
	}

	@summary = "Default summary"
	macro navItemNotPresent(navItem = null) {
		AssertElementNotPresent(
			key_navItem = ${navItem},
			locator1 = "NavBar#NAV_ITEM_REDIRECTION");
	}

	@summary = "Default summary"
	macro selectNewsTitle(title = null) {
		Click(
			key_title = ${title},
			locator1 = "EVPHome#NEWS_TITLE");

		WaitForElementPresent(locator1 = "Modal#BODY");
	}

}